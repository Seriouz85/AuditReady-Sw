  "private": true,
  "version": "1.1.0",
  "type": "module",
  "homepage": "https://seriouz85.github.io/audit-readiness-hub",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:github": "GITHUB_PAGES=true vite build --mode github",
    "build:vercel": "vite build --mode vercel",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "npm run build && git add dist -f && git commit -m 'Deploy build' && git subtree push --prefix dist origin main --force",
    "deploy:force": "npm run build && git add . && git commit -m 'Force deploy to main' && git push origin main --force",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "populate-iso": "node scripts/populate-iso-standards.js",
    "migrate:guidance-content": "tsx scripts/migrate-guidance-content.ts",
    "version:patch": "node scripts/version.js patch",
    "version:minor": "node scripts/version.js minor",
    "version:major": "node scripts/version.js major",
    "version:release": "node scripts/version.js patch --tag --push",
    "docker:build": "docker build -f Dockerfile.dev -t audit-readiness-hub:latest .",
    "docker:build:prod": "docker build -f Dockerfile.dev -t audit-readiness-hub:$(node -p \"require('./package.json').version\") --target production .",
    "docker:tag": "docker tag audit-readiness-hub:latest audit-readiness-hub:$(node -p \"require('./package.json').version\")"
  },
