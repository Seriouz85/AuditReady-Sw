20250702092615	{"-- Update timestamps to force React Query cache invalidation\n-- This will help resolve any stale cache issues causing infinite loading\n\nUPDATE unified_compliance_categories \nSET updated_at = now() \nWHERE is_active = true;\n\nUPDATE unified_requirements \nSET updated_at = now() \nWHERE id IN (\n  SELECT ur.id \n  FROM unified_requirements ur \n  JOIN unified_compliance_categories uc ON ur.category_id = uc.id \n  WHERE uc.is_active = true\n);\n\n-- Also check if there are any problematic NULL values that might cause issues\nUPDATE unified_requirements \nSET sub_requirements = ARRAY[]::text[]\nWHERE sub_requirements IS NULL;\n\nUPDATE unified_compliance_categories \nSET description = COALESCE(description, name)\nWHERE description IS NULL OR description = '';"}	fix_compliance_simplification_loading_issue	payam.razifar@gmail.com	\N
20250702092929	{"-- Remove CIS Chapter 12 controls from Network Monitoring & Defense\n-- They should ONLY be in Network Infrastructure Management\n\n-- Get the Network Monitoring & Defense unified requirement ID\nWITH network_monitoring_ur AS (\n  SELECT ur.id\n  FROM unified_requirements ur\n  JOIN unified_compliance_categories uc ON ur.category_id = uc.id\n  WHERE uc.name = 'Network Monitoring & Defense'\n  LIMIT 1\n)\n\n-- Delete mappings of CIS 12.x controls from Network Monitoring & Defense\nDELETE FROM unified_requirement_mappings\nWHERE unified_requirement_id IN (SELECT id FROM network_monitoring_ur)\n  AND requirement_id IN (\n    SELECT r.id \n    FROM requirements_library r\n    JOIN standards_library s ON r.standard_id = s.id\n    WHERE s.name LIKE '%CIS%' \n    AND r.control_id LIKE '12.%'\n  );"}	fix_cis_chapter_12_mappings_to_infrastructure_only	payam.razifar@gmail.com	\N
