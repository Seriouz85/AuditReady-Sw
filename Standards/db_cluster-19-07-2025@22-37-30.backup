20250608121826	{"-- Create organization_requirements table to track customer-specific requirement states\nCREATE TABLE IF NOT EXISTS organization_requirements (\n  id UUID DEFAULT gen_random_uuid() PRIMARY KEY,\n  organization_id UUID REFERENCES organizations(id) ON DELETE CASCADE,\n  requirement_id UUID REFERENCES requirements_library(id) ON DELETE CASCADE,\n  \n  -- Fulfillment tracking\n  status TEXT DEFAULT 'not-started' CHECK (status IN ('not-started', 'in-progress', 'fulfilled', 'partial', 'not-applicable')),\n  fulfillment_percentage INTEGER DEFAULT 0 CHECK (fulfillment_percentage >= 0 AND fulfillment_percentage <= 100),\n  \n  -- Evidence and documentation\n  evidence TEXT,\n  evidence_files JSONB DEFAULT '[]'::jsonb,\n  notes TEXT,\n  \n  -- Responsibility and dates\n  responsible_party TEXT,\n  assigned_to UUID REFERENCES auth.users(id),\n  due_date DATE,\n  last_assessment_date TIMESTAMPTZ,\n  next_review_date DATE,\n  \n  -- Tags and metadata\n  tags TEXT[] DEFAULT '{}',\n  custom_fields JSONB DEFAULT '{}'::jsonb,\n  \n  -- Risk and priority\n  risk_level TEXT CHECK (risk_level IN ('low', 'medium', 'high', 'critical')),\n  priority_override TEXT CHECK (priority_override IN ('low', 'medium', 'high', 'critical')),\n  \n  -- Audit trail\n  created_at TIMESTAMPTZ DEFAULT NOW(),\n  updated_at TIMESTAMPTZ DEFAULT NOW(),\n  created_by UUID REFERENCES auth.users(id),\n  updated_by UUID REFERENCES auth.users(id),\n  \n  UNIQUE(organization_id, requirement_id)\n);\n\n-- Create organization_standards table to track which standards an organization is using\nCREATE TABLE IF NOT EXISTS organization_standards (\n  id UUID DEFAULT gen_random_uuid() PRIMARY KEY,\n  organization_id UUID REFERENCES organizations(id) ON DELETE CASCADE,\n  standard_id UUID REFERENCES standards_library(id) ON DELETE CASCADE,\n  \n  -- Standard adoption tracking\n  is_active BOOLEAN DEFAULT true,\n  adoption_date DATE DEFAULT CURRENT_DATE,\n  target_compliance_date DATE,\n  current_compliance_percentage INTEGER DEFAULT 0,\n  \n  -- Settings\n  settings JSONB DEFAULT '{}'::jsonb,\n  \n  created_at TIMESTAMPTZ DEFAULT NOW(),\n  updated_at TIMESTAMPTZ DEFAULT NOW(),\n  \n  UNIQUE(organization_id, standard_id)\n);\n\n-- Create requirement_evidence table for detailed evidence tracking\nCREATE TABLE IF NOT EXISTS requirement_evidence (\n  id UUID DEFAULT gen_random_uuid() PRIMARY KEY,\n  organization_requirement_id UUID REFERENCES organization_requirements(id) ON DELETE CASCADE,\n  \n  title TEXT NOT NULL,\n  description TEXT,\n  evidence_type TEXT CHECK (evidence_type IN ('document', 'screenshot', 'link', 'text', 'approval', 'test-result')),\n  \n  -- File information\n  file_url TEXT,\n  file_name TEXT,\n  file_size INTEGER,\n  mime_type TEXT,\n  \n  -- Metadata\n  collected_date DATE DEFAULT CURRENT_DATE,\n  expires_date DATE,\n  collected_by UUID REFERENCES auth.users(id),\n  \n  -- Validation\n  is_validated BOOLEAN DEFAULT false,\n  validated_by UUID REFERENCES auth.users(id),\n  validated_at TIMESTAMPTZ,\n  validation_notes TEXT,\n  \n  created_at TIMESTAMPTZ DEFAULT NOW(),\n  updated_at TIMESTAMPTZ DEFAULT NOW()\n);\n\n-- Create indexes for performance\nCREATE INDEX idx_org_req_organization ON organization_requirements(organization_id);\nCREATE INDEX idx_org_req_requirement ON organization_requirements(requirement_id);\nCREATE INDEX idx_org_req_status ON organization_requirements(status);\nCREATE INDEX idx_org_std_organization ON organization_standards(organization_id);\nCREATE INDEX idx_evidence_org_req ON requirement_evidence(organization_requirement_id);\n\n-- Enable RLS\nALTER TABLE organization_requirements ENABLE ROW LEVEL SECURITY;\nALTER TABLE organization_standards ENABLE ROW LEVEL SECURITY;\nALTER TABLE requirement_evidence ENABLE ROW LEVEL SECURITY;\n\n-- RLS Policies for organization_requirements\nCREATE POLICY \\"Users can view their organization's requirements\\" ON organization_requirements\n  FOR SELECT TO authenticated\n  USING (\n    organization_id IN (\n      SELECT organization_id FROM organization_users \n      WHERE user_id = auth.uid() AND status = 'active'\n    )\n  );\n\nCREATE POLICY \\"Users can update their organization's requirements\\" ON organization_requirements\n  FOR UPDATE TO authenticated\n  USING (\n    organization_id IN (\n      SELECT organization_id FROM organization_users \n      WHERE user_id = auth.uid() AND status = 'active'\n    )\n  );\n\n-- Trigger to update updated_at timestamp\nCREATE OR REPLACE FUNCTION update_updated_at_column()\nRETURNS TRIGGER AS $$\nBEGIN\n  NEW.updated_at = NOW();\n  RETURN NEW;\nEND;\n$$ language 'plpgsql';\n\nCREATE TRIGGER update_organization_requirements_updated_at BEFORE UPDATE ON organization_requirements\n  FOR EACH ROW EXECUTE FUNCTION update_updated_at_column();\n\nCREATE TRIGGER update_organization_standards_updated_at BEFORE UPDATE ON organization_standards\n  FOR EACH ROW EXECUTE FUNCTION update_updated_at_column();"}	create_customer_requirements_tables	payam.razifar@gmail.com	\N
20250701065809	{"-- Quick test to see if our unified categories have requirements\nSELECT \n  c.name,\n  count(r.*) as requirement_count\nFROM unified_compliance_categories c\nLEFT JOIN unified_requirements r ON c.id = r.category_id AND r.is_active = true\nWHERE c.is_active = true\nGROUP BY c.id, c.name\nORDER BY c.sort_order\nLIMIT 5;"}	test_unified_data	payam.razifar@gmail.com	\N
20250608040710	{"-- Create missing enums first\nDO $$ \nBEGIN\n    IF NOT EXISTS (SELECT 1 FROM pg_type WHERE typname = 'requirement_priority') THEN\n        CREATE TYPE requirement_priority AS ENUM ('low', 'medium', 'high', 'critical');\n    END IF;\nEND $$;\n\n-- Organization-specific requirement overrides for guidance and assignments\nCREATE TABLE organization_requirement_overrides (\n    id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),\n    organization_id UUID REFERENCES organizations(id) ON DELETE CASCADE,\n    requirement_id UUID REFERENCES requirements_library(id) ON DELETE CASCADE,\n    custom_guidance TEXT, -- Organization's custom guidance text\n    assigned_user_id UUID REFERENCES organization_users(id) ON DELETE SET NULL,\n    organization_tags TEXT[], -- Organization-specific tags\n    custom_priority requirement_priority,\n    custom_due_date DATE,\n    is_active BOOLEAN DEFAULT true,\n    created_at TIMESTAMP DEFAULT NOW(),\n    updated_at TIMESTAMP DEFAULT NOW(),\n    created_by UUID REFERENCES organization_users(id),\n    updated_by UUID REFERENCES organization_users(id),\n    UNIQUE(organization_id, requirement_id)\n);\n\n-- User assignments for requirements\nCREATE TABLE user_requirement_assignments (\n    id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),\n    organization_id UUID REFERENCES organizations(id) ON DELETE CASCADE,\n    user_id UUID REFERENCES organization_users(id) ON DELETE CASCADE,\n    requirement_id UUID REFERENCES requirements_library(id) ON DELETE CASCADE,\n    assigned_by UUID REFERENCES organization_users(id),\n    due_date DATE,\n    status TEXT DEFAULT 'assigned', -- 'assigned', 'in_progress', 'completed', 'overdue'\n    notes TEXT,\n    created_at TIMESTAMP DEFAULT NOW(),\n    updated_at TIMESTAMP DEFAULT NOW(),\n    UNIQUE(organization_id, user_id, requirement_id)\n);\n\n-- Activity tracking\nCREATE TABLE user_activities (\n    id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),\n    organization_id UUID REFERENCES organizations(id) ON DELETE CASCADE,\n    user_id UUID REFERENCES organization_users(id) ON DELETE CASCADE,\n    activity_type TEXT NOT NULL, -- 'assignment', 'completion', 'update', 'comment'\n    entity_type TEXT NOT NULL, -- 'requirement', 'assessment', 'guidance'\n    entity_id UUID NOT NULL,\n    description TEXT NOT NULL,\n    metadata JSONB,\n    created_at TIMESTAMP DEFAULT NOW()\n);\n\n-- Indexes for performance\nCREATE INDEX idx_org_requirement_overrides_org ON organization_requirement_overrides(organization_id);\nCREATE INDEX idx_org_requirement_overrides_req ON organization_requirement_overrides(requirement_id);\nCREATE INDEX idx_user_assignments_user ON user_requirement_assignments(user_id);\nCREATE INDEX idx_user_assignments_org ON user_requirement_assignments(organization_id);\nCREATE INDEX idx_user_activities_user ON user_activities(user_id);\nCREATE INDEX idx_user_activities_org ON user_activities(organization_id);\n\n-- RLS policies\nALTER TABLE organization_requirement_overrides ENABLE ROW LEVEL SECURITY;\nALTER TABLE user_requirement_assignments ENABLE ROW LEVEL SECURITY;\nALTER TABLE user_activities ENABLE ROW LEVEL SECURITY;\n\n-- Organization users can only access their organization's data\nCREATE POLICY \\"Org users access own overrides\\" ON organization_requirement_overrides\n    FOR ALL TO authenticated\n    USING (\n        organization_id IN (\n            SELECT ou.organization_id FROM organization_users ou\n            JOIN auth.users u ON u.id = ou.user_id\n            WHERE u.email = auth.email() AND ou.status = 'active'\n        )\n        OR EXISTS (\n            SELECT 1 FROM platform_administrators \n            WHERE email = (SELECT email FROM auth.users WHERE id = auth.uid()) AND is_active = true\n        )\n    );\n\nCREATE POLICY \\"Org users access own assignments\\" ON user_requirement_assignments\n    FOR ALL TO authenticated\n    USING (\n        organization_id IN (\n            SELECT ou.organization_id FROM organization_users ou\n            JOIN auth.users u ON u.id = ou.user_id\n            WHERE u.email = auth.email() AND ou.status = 'active'\n        )\n        OR EXISTS (\n            SELECT 1 FROM platform_administrators \n            WHERE email = (SELECT email FROM auth.users WHERE id = auth.uid()) AND is_active = true\n        )\n    );\n\nCREATE POLICY \\"Org users access own activities\\" ON user_activities\n    FOR ALL TO authenticated\n    USING (\n        organization_id IN (\n            SELECT ou.organization_id FROM organization_users ou\n            JOIN auth.users u ON u.id = ou.user_id\n            WHERE u.email = auth.email() AND ou.status = 'active'\n        )\n        OR EXISTS (\n            SELECT 1 FROM platform_administrators \n            WHERE email = (SELECT email FROM auth.users WHERE id = auth.uid()) AND is_active = true\n        )\n    );\n\n-- Triggers for updated_at\nCREATE TRIGGER update_org_requirement_overrides_updated_at \n    BEFORE UPDATE ON organization_requirement_overrides \n    FOR EACH ROW EXECUTE FUNCTION update_updated_at_column();\n\nCREATE TRIGGER update_user_assignments_updated_at \n    BEFORE UPDATE ON user_requirement_assignments \n    FOR EACH ROW EXECUTE FUNCTION update_updated_at_column();"}	004_org_requirement_overrides_correct	payam.razifar@gmail.com	\N
20250610045327	{"-- ============================================================================\n-- Remove unwanted standards: NIS2 and ISO 27005\n-- Clean up duplicate standards and keep only the 7 required standards\n-- ============================================================================\n\n-- First remove all requirements for standards we're going to delete\nDELETE FROM requirements_library \nWHERE standard_id IN (\n    SELECT id FROM standards_library \n    WHERE name IN ('NIS2 Directive', 'ISO/IEC 27005')\n);\n\n-- Remove the unwanted standards themselves\nDELETE FROM standards_library \nWHERE name IN ('NIS2 Directive', 'ISO/IEC 27005');\n\n-- Remove duplicate/incorrect versions and keep only the correct ones\n-- Remove CIS Controls with version \\"8.1.2\\" (keep \\"v8.1\\" versions which have requirements)\nDELETE FROM requirements_library \nWHERE standard_id IN (\n    SELECT id FROM standards_library \n    WHERE name LIKE 'CIS Controls%' AND version = '8.1.2'\n);\n\nDELETE FROM standards_library \nWHERE name LIKE 'CIS Controls%' AND version = '8.1.2';\n\n-- Remove GDPR version \\"2018\\" (keep \\"2016/679\\" which has requirements)\nDELETE FROM requirements_library \nWHERE standard_id IN (\n    SELECT id FROM standards_library \n    WHERE name = 'GDPR' AND version = '2018'\n);\n\nDELETE FROM standards_library \nWHERE name = 'GDPR' AND version = '2018';\n\n-- Update remaining standard names to match mockData.ts format\nUPDATE standards_library \nSET name = 'CIS Controls Implementation Group 1 (IG1)',\n    version = '8.1.2',\n    description = 'Essential cyber hygiene safeguards that every enterprise should implement. IG1 contains 56 foundational safeguards for basic cyber defense readiness.'\nWHERE name = 'CIS Controls IG1' AND version = 'v8.1';\n\nUPDATE standards_library \nSET name = 'CIS Controls Implementation Group 2 (IG2)',\n    version = '8.1.2', \n    description = 'All IG1 safeguards plus additional safeguards for organizations with moderate cybersecurity maturity and resources. IG2 contains 130 total safeguards.'\nWHERE name = 'CIS Controls IG2' AND version = 'v8.1';\n\nUPDATE standards_library \nSET name = 'CIS Controls Implementation Group 3 (IG3)',\n    version = '8.1.2',\n    description = 'All IG1 and IG2 safeguards plus additional safeguards for organizations with high cybersecurity maturity and significant resources. IG3 contains all 153 safeguards.'\nWHERE name = 'CIS Controls IG3' AND version = 'v8.1';\n\n-- Update GDPR to match mockData.ts format\nUPDATE standards_library \nSET version = '2018',\n    description = 'General Data Protection Regulation for data protection and privacy in the EU.'\nWHERE name = 'GDPR' AND version = '2016/679';"}	remove_unwanted_standards	payam.razifar@gmail.com	\N
20250611071951	{"-- Fix RLS Security Issues Identified in Audit (Corrected)\n-- This migration addresses all RLS disabled and missing policy issues\n\n-- 1. Enable RLS on tables that are missing it\nALTER TABLE public.user_roles ENABLE ROW LEVEL SECURITY;\nALTER TABLE public.organization_users ENABLE ROW LEVEL SECURITY;\nALTER TABLE public.platform_administrators ENABLE ROW LEVEL SECURITY;\nALTER TABLE public.standards_library ENABLE ROW LEVEL SECURITY;\nALTER TABLE public.requirements_library ENABLE ROW LEVEL SECURITY;\nALTER TABLE public.admin_permissions ENABLE ROW LEVEL SECURITY;\nALTER TABLE public.enhanced_audit_logs ENABLE ROW LEVEL SECURITY;\nALTER TABLE public.feature_flags ENABLE ROW LEVEL SECURITY;\nALTER TABLE public.enhanced_user_invitations ENABLE ROW LEVEL SECURITY;\nALTER TABLE public.user_sessions ENABLE ROW LEVEL SECURITY;\nALTER TABLE public.system_settings ENABLE ROW LEVEL SECURITY;\n\n-- 2. Add RLS policies for user_roles (role definitions - publicly viewable)\nCREATE POLICY \\"Authenticated users can view roles\\" ON public.user_roles\n    FOR SELECT USING (auth.role() = 'authenticated');\n\nCREATE POLICY \\"Platform admins can manage roles\\" ON public.user_roles\n    FOR ALL USING (\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\n-- 3. Add RLS policies for organization_users\nCREATE POLICY \\"Users can view their own organization membership\\" ON public.organization_users\n    FOR SELECT USING (auth.uid() = user_id);\n\nCREATE POLICY \\"Organization admins can manage users in their org\\" ON public.organization_users\n    FOR ALL USING (\n        -- User is an admin/owner in the same organization\n        EXISTS (\n            SELECT 1 FROM public.organization_users ou\n            INNER JOIN public.user_roles ur ON ou.role_id = ur.id\n            WHERE ou.user_id = auth.uid()\n            AND ou.organization_id = organization_users.organization_id\n            AND ur.name IN ('admin', 'owner')\n        ) OR\n        -- Or user is a platform admin\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\n-- 4. Add RLS policies for platform_administrators (highly restricted)\nCREATE POLICY \\"Platform admins can view admin records\\" ON public.platform_administrators\n    FOR SELECT USING (\n        email = auth.jwt() ->> 'email' AND is_active = true\n    );\n\n-- Platform admin changes require superuser access only\nCREATE POLICY \\"Platform admin management restricted\\" ON public.platform_administrators\n    FOR ALL USING (false);\n\n-- 5. Add RLS policies for standards_library\nCREATE POLICY \\"Authenticated users can view standards\\" ON public.standards_library\n    FOR SELECT USING (auth.role() = 'authenticated');\n\nCREATE POLICY \\"Platform admins can manage standards\\" ON public.standards_library\n    FOR ALL USING (\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\n-- 6. Add RLS policies for requirements_library  \nCREATE POLICY \\"Authenticated users can view requirements\\" ON public.requirements_library\n    FOR SELECT USING (auth.role() = 'authenticated');\n\nCREATE POLICY \\"Platform admins can manage requirements\\" ON public.requirements_library\n    FOR ALL USING (\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\n-- 7. Add RLS policies for admin_permissions\nCREATE POLICY \\"Authenticated users can view permissions\\" ON public.admin_permissions\n    FOR SELECT USING (auth.role() = 'authenticated');\n\nCREATE POLICY \\"Platform admins can manage permissions\\" ON public.admin_permissions\n    FOR ALL USING (\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\n-- 8. Add RLS policies for enhanced_audit_logs\nCREATE POLICY \\"Platform admins can view audit logs\\" ON public.enhanced_audit_logs\n    FOR SELECT USING (\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\nCREATE POLICY \\"System can insert audit logs\\" ON public.enhanced_audit_logs\n    FOR INSERT WITH CHECK (true);\n\n-- 9. Add RLS policies for feature_flags\nCREATE POLICY \\"Authenticated users can view global feature flags\\" ON public.feature_flags\n    FOR SELECT USING (\n        auth.role() = 'authenticated' AND organization_id IS NULL\n    );\n\nCREATE POLICY \\"Organization members can view org feature flags\\" ON public.feature_flags\n    FOR SELECT USING (\n        organization_id IS NOT NULL AND\n        EXISTS (\n            SELECT 1 FROM public.organization_users \n            WHERE user_id = auth.uid() \n            AND organization_id = feature_flags.organization_id\n        )\n    );\n\nCREATE POLICY \\"Platform admins can manage feature flags\\" ON public.feature_flags\n    FOR ALL USING (\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\n-- 10. Add RLS policies for enhanced_user_invitations\nCREATE POLICY \\"Users can view invitations sent to them\\" ON public.enhanced_user_invitations\n    FOR SELECT USING (email = auth.jwt() ->> 'email');\n\nCREATE POLICY \\"Organization admins can manage invitations\\" ON public.enhanced_user_invitations\n    FOR ALL USING (\n        -- User is admin/owner in the organization\n        EXISTS (\n            SELECT 1 FROM public.organization_users ou\n            INNER JOIN public.user_roles ur ON ou.role_id = ur.id\n            WHERE ou.user_id = auth.uid()\n            AND ou.organization_id = enhanced_user_invitations.organization_id\n            AND ur.name IN ('admin', 'owner')\n        ) OR\n        -- Or user is a platform admin\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\n-- 11. Add RLS policies for user_sessions\nCREATE POLICY \\"Users can view their own sessions\\" ON public.user_sessions\n    FOR SELECT USING (user_id = auth.uid());\n\nCREATE POLICY \\"Users can manage their own sessions\\" ON public.user_sessions\n    FOR ALL USING (user_id = auth.uid());\n\nCREATE POLICY \\"Platform admins can view all sessions\\" ON public.user_sessions\n    FOR SELECT USING (\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\n-- 12. Add RLS policies for system_settings\nCREATE POLICY \\"Users can view public system settings\\" ON public.system_settings\n    FOR SELECT USING (auth.role() = 'authenticated' AND is_public = true);\n\nCREATE POLICY \\"Platform admins can manage system settings\\" ON public.system_settings\n    FOR ALL USING (\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\n-- 13. Add missing policies for organization_standards (RLS enabled but no policies)\nCREATE POLICY \\"Organization members can view their standards\\" ON public.organization_standards\n    FOR SELECT USING (\n        EXISTS (\n            SELECT 1 FROM public.organization_users \n            WHERE user_id = auth.uid()\n            AND organization_id = organization_standards.organization_id\n        )\n    );\n\nCREATE POLICY \\"Organization admins can manage their standards\\" ON public.organization_standards\n    FOR ALL USING (\n        -- User is admin/owner in the organization\n        EXISTS (\n            SELECT 1 FROM public.organization_users ou\n            INNER JOIN public.user_roles ur ON ou.role_id = ur.id\n            WHERE ou.user_id = auth.uid()\n            AND ou.organization_id = organization_standards.organization_id\n            AND ur.name IN ('admin', 'owner')\n        ) OR\n        -- Or user is a platform admin\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\n-- 14. Add missing policies for requirement_evidence (RLS enabled but no policies)\n-- Evidence is linked to organization via organization_requirements\nCREATE POLICY \\"Organization members can view their evidence\\" ON public.requirement_evidence\n    FOR SELECT USING (\n        EXISTS (\n            SELECT 1 FROM public.organization_requirements oq\n            INNER JOIN public.organization_users ou ON ou.organization_id = oq.organization_id\n            WHERE ou.user_id = auth.uid()\n            AND oq.id = requirement_evidence.organization_requirement_id\n        )\n    );\n\nCREATE POLICY \\"Organization users can manage evidence for their requirements\\" ON public.requirement_evidence\n    FOR ALL USING (\n        -- User is member of organization that owns the requirement\n        EXISTS (\n            SELECT 1 FROM public.organization_requirements oq\n            INNER JOIN public.organization_users ou ON ou.organization_id = oq.organization_id\n            WHERE ou.user_id = auth.uid()\n            AND oq.id = requirement_evidence.organization_requirement_id\n        ) OR\n        -- Or user is a platform admin\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\n-- 15. Fix function security by setting search_path (immutable)\n-- Recreate functions with proper security settings\n\nCREATE OR REPLACE FUNCTION public.get_user_permissions(user_email text)\nRETURNS json\nLANGUAGE plpgsql\nSECURITY DEFINER\nSET search_path = public\nSTABLE\nAS $$\nDECLARE\n    result json;\nBEGIN\n    SELECT json_agg(\n        json_build_object(\n            'permission', jsonb_array_elements_text(ur.permissions),\n            'role', ur.name,\n            'organization_id', ou.organization_id\n        )\n    ) INTO result\n    FROM public.organization_users ou\n    INNER JOIN public.user_roles ur ON ou.role_id = ur.id\n    INNER JOIN auth.users u ON u.id = ou.user_id\n    WHERE u.email = user_email;\n    \n    RETURN COALESCE(result, '[]'::json);\nEND;\n$$;\n\nCREATE OR REPLACE FUNCTION public.update_system_settings_timestamp()\nRETURNS trigger\nLANGUAGE plpgsql\nSECURITY DEFINER\nSET search_path = public\nAS $$\nBEGIN\n    NEW.updated_at = NOW();\n    RETURN NEW;\nEND;\n$$;\n\nCREATE OR REPLACE FUNCTION public.log_activity(\n    p_user_id uuid,\n    p_organization_id uuid,\n    p_action text,\n    p_details jsonb DEFAULT '{}'::jsonb\n)\nRETURNS void\nLANGUAGE plpgsql\nSECURITY DEFINER\nSET search_path = public\nAS $$\nBEGIN\n    INSERT INTO public.enhanced_audit_logs (\n        user_id,\n        organization_id,\n        action,\n        resource_type,\n        new_values,\n        created_at\n    ) VALUES (\n        p_user_id,\n        p_organization_id,\n        p_action,\n        'system',\n        p_details,\n        NOW()\n    );\nEND;\n$$;\n\nCREATE OR REPLACE FUNCTION public.update_updated_at_column()\nRETURNS trigger\nLANGUAGE plpgsql\nSECURITY DEFINER\nSET search_path = public\nAS $$\nBEGIN\n    NEW.updated_at = NOW();\n    RETURN NEW;\nEND;\n$$;\n\n-- Grant necessary permissions for service role to bypass RLS for admin operations\nGRANT USAGE ON SCHEMA public TO service_role;\nGRANT ALL ON ALL TABLES IN SCHEMA public TO service_role;\nGRANT ALL ON ALL SEQUENCES IN SCHEMA public TO service_role;\nGRANT ALL ON ALL FUNCTIONS IN SCHEMA public TO service_role;\n\n-- Add helpful comments\nCOMMENT ON POLICY \\"Users can view their own organization membership\\" ON public.organization_users IS 'Users can see their own role assignments';\nCOMMENT ON POLICY \\"Organization members can view their standards\\" ON public.organization_standards IS 'Members can view standards applied to their organization';\nCOMMENT ON POLICY \\"Platform admins can view audit logs\\" ON public.enhanced_audit_logs IS 'Platform administrators need access to audit logs for security monitoring';\nCOMMENT ON POLICY \\"Organization members can view their evidence\\" ON public.requirement_evidence IS 'Organization members can view evidence for requirements in their organization';"}	fix_rls_security_issues_corrected	payam.razifar@gmail.com	\N
20250611073004	{"-- Fix organizations table RLS and create test data\n\n-- Add RLS policies for organizations table (missing from previous migration)\nCREATE POLICY \\"Platform admins can view all organizations\\" ON public.organizations\n    FOR SELECT USING (\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\nCREATE POLICY \\"Platform admins can manage organizations\\" ON public.organizations\n    FOR ALL USING (\n        EXISTS (\n            SELECT 1 FROM public.platform_administrators \n            WHERE email = auth.jwt() ->> 'email' AND is_active = true\n        )\n    );\n\nCREATE POLICY \\"Organization members can view their organization\\" ON public.organizations\n    FOR SELECT USING (\n        EXISTS (\n            SELECT 1 FROM public.organization_users \n            WHERE user_id = auth.uid() \n            AND organization_id = organizations.id\n        )\n    );\n\n-- Create some test organizations for the admin console\nINSERT INTO public.organizations (id, name, slug, industry, company_size, subscription_tier, settings, created_at, updated_at) VALUES \n(\n    gen_random_uuid(),\n    'Demo Corporation',\n    'demo-corp',\n    'Technology',\n    '50-200',\n    'professional',\n    '{\\"features\\": [\\"all\\"], \\"limits\\": {\\"users\\": 50, \\"assessments\\": 10}}',\n    NOW(),\n    NOW()\n),\n(\n    gen_random_uuid(),\n    'Enterprise Solutions Ltd',\n    'enterprise-solutions',\n    'Consulting',\n    '200-1000',\n    'enterprise',\n    '{\\"features\\": [\\"all\\"], \\"limits\\": {\\"users\\": 200, \\"assessments\\": 50}}',\n    NOW(),\n    NOW()\n),\n(\n    gen_random_uuid(),\n    'StartupTech Inc',\n    'startuptech',\n    'Software',\n    '10-50',\n    'starter',\n    '{\\"features\\": [\\"basic\\"], \\"limits\\": {\\"users\\": 10, \\"assessments\\": 3}}',\n    NOW(),\n    NOW()\n);\n\n-- Update system_settings to ensure public visibility for key settings\nUPDATE public.system_settings SET is_public = true \nWHERE key IN ('platform_name', 'platform_version', 'maintenance_mode', 'registration_enabled');\n\n-- Insert default system settings if they don't exist\nINSERT INTO public.system_settings (id, category, key, value, data_type, description, is_public, created_at, updated_at)\nVALUES \n    (gen_random_uuid(), 'platform', 'platform_name', '\\"AuditReady\\"', 'string', 'Platform name', true, NOW(), NOW()),\n    (gen_random_uuid(), 'platform', 'platform_version', '\\"1.0.0\\"', 'string', 'Platform version', true, NOW(), NOW()),\n    (gen_random_uuid(), 'platform', 'maintenance_mode', 'false', 'boolean', 'Maintenance mode status', true, NOW(), NOW()),\n    (gen_random_uuid(), 'stripe', 'stripe_enabled', 'true', 'boolean', 'Stripe integration enabled', false, NOW(), NOW())\nON CONFLICT (category, key) DO NOTHING;\n\n-- Grant service role permissions to bypass RLS for admin operations\nALTER TABLE public.organizations FORCE ROW LEVEL SECURITY;\n\n-- Add helpful indexes for performance\nCREATE INDEX IF NOT EXISTS idx_organizations_subscription_tier ON public.organizations(subscription_tier);\nCREATE INDEX IF NOT EXISTS idx_organizations_created_at ON public.organizations(created_at);\nCREATE INDEX IF NOT EXISTS idx_platform_administrators_email ON public.platform_administrators(email) WHERE is_active = true;"}	fix_organizations_rls_and_access	payam.razifar@gmail.com	\N
