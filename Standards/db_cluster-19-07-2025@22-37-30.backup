20250701074606	{"-- Clear existing ISO 27002 mappings and create comprehensive real mappings\nDELETE FROM unified_requirement_mappings WHERE requirement_id IN (\n  SELECT r.id FROM requirements_library r\n  JOIN standards_library s ON r.standard_id = s.id\n  WHERE s.name LIKE '%ISO/IEC 27002%'\n);\n\n-- Helper function to get unified requirement ID by category name\nCREATE OR REPLACE FUNCTION get_unified_req_by_category(cat_name TEXT)\nRETURNS UUID AS $$\n  SELECT ur.id \n  FROM unified_requirements ur\n  JOIN unified_compliance_categories ucc ON ur.category_id = ucc.id\n  WHERE ucc.name = cat_name\n  LIMIT 1;\n$$ LANGUAGE SQL;\n\n-- Map ISO 27002 A.5.x (Organizational controls) to Governance & Leadership\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Governance & Leadership'),\n  r.id,\n  'strong',\n  'ISO 27002 organizational controls'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A5.1','A5.2','A5.3','A5.4','A5.5','A5.6','A5.7','A5.8','A5.9','A5.10','A5.11','A5.31','A5.32','A5.33','A5.34','A5.35','A5.36','A5.37');\n\n-- Map ISO 27002 A.6.x (People controls) to Security Awareness & Skills Training  \nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Security Awareness & Skills Training'),\n  r.id,\n  'exact',\n  'ISO 27002 people controls'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id LIKE 'A6.%';\n\n-- Map ISO 27002 A.7.x (Physical controls) to Physical & Environmental Security\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Physical & Environmental Security'),\n  r.id,\n  'exact',\n  'ISO 27002 physical and environmental controls'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id LIKE 'A7.%';\n\n-- Map ISO 27002 Identity & Access Management controls\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Identity & Access Management'),\n  r.id,\n  'exact',\n  'ISO 27002 access control and identity management'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A5.15','A5.16','A5.17','A5.18','A8.1','A8.2','A8.3','A8.4','A8.5');\n\n-- Map ISO 27002 Data Protection controls\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Data Protection'),\n  r.id,\n  'exact',\n  'ISO 27002 data protection and cryptography'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A5.12','A5.13','A5.14','A8.24','A8.10','A8.11','A8.12');\n\n-- Map ISO 27002 Configuration Management \nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Secure Configuration of Hardware and Software'),\n  r.id,\n  'exact',\n  'ISO 27002 configuration management'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A8.9','A8.6','A8.13','A8.14','A8.19');\n\n-- Map ISO 27002 Network Security controls\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Network Infrastructure Management'),\n  r.id,\n  'exact',\n  'ISO 27002 network security controls'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A8.20','A8.21','A8.22','A8.23','A5.23');\n\n-- Map ISO 27002 Secure Development controls\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Secure Software Development'),\n  r.id,\n  'exact',\n  'ISO 27002 secure development controls'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A8.25','A8.26','A8.27','A8.28','A8.29','A8.30','A8.31','A8.33','A8.15');\n\n-- Map ISO 27002 Incident Management controls\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Incident Response & Security Event Management'),\n  r.id,\n  'exact',\n  'ISO 27002 incident management controls'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A5.24','A5.25','A5.26','A5.27','A6.8');\n\n-- Map ISO 27002 Supplier Management controls  \nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Supplier & Third-Party Risk Management'),\n  r.id,\n  'exact',\n  'ISO 27002 supplier relationship controls'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A5.19','A5.20','A5.21','A5.22','A5.23');\n\n-- Map ISO 27002 Vulnerability and Monitoring controls\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Continuous Vulnerability Management'),\n  r.id,\n  'exact',\n  'ISO 27002 vulnerability management'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A8.8','A5.7');\n\n-- Map ISO 27002 Monitoring controls\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Audit Log Management'),\n  r.id,\n  'exact',\n  'ISO 27002 monitoring and logging'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A8.16');\n\n-- Map ISO 27002 Malware Protection\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Malware Defenses'),\n  r.id,\n  'exact',\n  'ISO 27002 malware protection'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A8.7');\n\n-- Map ISO 27002 Business Continuity\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Business Continuity & Disaster Recovery Management'),\n  r.id,\n  'exact',\n  'ISO 27002 business continuity'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A5.29','A5.30');\n\n-- Map ISO 27002 Operational controls\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Governance & Leadership'),\n  r.id,\n  'strong',\n  'ISO 27002 operational procedures'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%' \nAND r.control_id IN ('A8.1');\n\n-- Clean up helper function\nDROP FUNCTION IF EXISTS get_unified_req_by_category(TEXT);"}	complete_iso27002_mapping	payam.razifar@gmail.com	\N
20250701075438	{"-- REMOVE ALL MAPPINGS and start fresh with ONLY real requirements\nDELETE FROM unified_requirement_mappings;\n\n-- Helper function\nCREATE OR REPLACE FUNCTION get_unified_req_by_category(cat_name TEXT)\nRETURNS UUID AS $$\n  SELECT ur.id \n  FROM unified_requirements ur\n  JOIN unified_compliance_categories ucc ON ur.category_id = ucc.id\n  WHERE ucc.name = cat_name\n  LIMIT 1;\n$$ LANGUAGE SQL;\n\n-- Map ONLY the real ISO 27001 requirements (30 total) to Governance & Leadership\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Governance & Leadership'),\n  r.id,\n  'strong',\n  'ISO 27001 management system requirement'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27001%';\n\n-- Map ONLY the real ISO 27001 risk management clauses to Risk Management  \nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Risk Management'),\n  r.id,\n  'exact',\n  'ISO 27001 risk management clause'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27001%'\nAND r.control_id IN ('6.1.2', '6.1.3', '8.2', '8.3');\n\n-- Map ONLY real ISO 27002 A5.1-A5.11, A5.31-A5.37 to Governance & Leadership\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Governance & Leadership'),\n  r.id,\n  'strong',\n  'ISO 27002 organizational control'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%'\nAND (r.control_id IN ('A5.1','A5.2','A5.3','A5.4','A5.5','A5.10','A5.11') \n     OR r.control_id LIKE 'A5.3%');\n\n-- Map ONLY real ISO 27002 A6.x to Security Awareness & Skills Training\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Security Awareness & Skills Training'),\n  r.id,\n  'exact',\n  'ISO 27002 people control'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%'\nAND r.control_id LIKE 'A6.%';\n\n-- Map ONLY real ISO 27002 A7.x to Physical & Environmental Security\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Physical & Environmental Security'),\n  r.id,\n  'exact',\n  'ISO 27002 physical control'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%'\nAND r.control_id LIKE 'A7.%';\n\n-- Map ONLY real ISO 27002 Access Control requirements\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Identity & Access Management'),\n  r.id,\n  'exact',\n  'ISO 27002 access control'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%'\nAND r.control_id IN ('A5.15','A5.16','A5.17','A5.18','A5.19','A5.20','A5.21');\n\n-- Map ONLY real ISO 27002 Data Protection requirements\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Data Protection'),\n  r.id,\n  'exact',\n  'ISO 27002 data protection'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%'\nAND r.control_id IN ('A5.12','A5.13','A5.14','A5.22');\n\n-- Map ONLY real ISO 27002 Incident Response requirements\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Incident Response & Security Event Management'),\n  r.id,\n  'exact',\n  'ISO 27002 incident management'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%'\nAND r.control_id IN ('A5.24','A5.25','A5.26','A5.27');\n\n-- Map ONLY real ISO 27002 Cloud/Supplier requirements\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Supplier & Third-Party Risk Management'),\n  r.id,\n  'exact',\n  'ISO 27002 supplier/cloud control'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%ISO/IEC 27002%'\nAND r.control_id IN ('A5.23');\n\n-- Map ONLY real CIS Controls 1.x to Hardware Assets\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Inventory and Control of Hardware Assets'),\n  r.id,\n  'exact',\n  'CIS Chapter 1 - Hardware inventory'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%CIS Controls%'\nAND r.control_id LIKE '1.%';\n\n-- Map ONLY real CIS Controls 2.x to Software Assets\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('Inventory and Control of Software Assets'),\n  r.id,\n  'exact',\n  'CIS Chapter 2 - Software inventory'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%CIS Controls%'\nAND r.control_id LIKE '2.%';\n\n-- Map ONLY real GDPR requirements to GDPR Unified Compliance\nINSERT INTO unified_requirement_mappings (unified_requirement_id, requirement_id, mapping_strength, notes)\nSELECT \n  get_unified_req_by_category('GDPR Unified Compliance'),\n  r.id,\n  'exact',\n  'GDPR article'\nFROM requirements_library r\nJOIN standards_library s ON r.standard_id = s.id\nWHERE s.name LIKE '%GDPR%';\n\n-- Clean up\nDROP FUNCTION IF EXISTS get_unified_req_by_category(TEXT);"}	remove_fake_mappings_real_only	payam.razifar@gmail.com	\N
20250701080630	{"-- REVERT EVERYTHING - Delete all my fucked up mappings and tables\nDELETE FROM unified_requirement_mappings;\nDELETE FROM aggregated_requirements;\nDELETE FROM requirement_cluster_members;\nDELETE FROM requirement_clusters;\nDELETE FROM requirement_similarity_scores;\nDELETE FROM framework_control_mappings;\nDELETE FROM aggregation_rules;\n\n-- Drop the tables I added that fucked things up\nDROP TABLE IF EXISTS aggregated_requirements;\nDROP TABLE IF EXISTS requirement_cluster_members;\nDROP TABLE IF EXISTS requirement_clusters;\nDROP TABLE IF EXISTS requirement_similarity_scores;\nDROP TABLE IF EXISTS framework_control_mappings;\nDROP TABLE IF EXISTS aggregation_rules;"}	revert_to_original_state	payam.razifar@gmail.com	\N
20250701081920	{"-- Insert real ISO 27001 requirements with correct column structure\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- ISO 27001 Section 4 requirements\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '4.1', 'Understanding the organization and its context', 'The organization shall determine external and internal issues that are relevant to its purpose and that affect its ability to achieve the intended outcome(s) of its information security management system. Implementation: Conduct regular environmental scanning to identify external factors (regulatory changes, market conditions, threats). Assess internal factors (organizational structure, culture, capabilities, resources). Document how these factors impact information security objectives. Review and update context analysis at least annually.', 'Governance & Leadership', 'high', 1, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '4.2', 'Understanding the needs and expectations of interested parties', 'The organization shall determine: a) interested parties that are relevant to the information security management system; b) the relevant requirements of these interested parties; c) which of these requirements will be addressed through the information security management system. Implementation: Create stakeholder register with all interested parties. Document specific security requirements from each stakeholder group. Map requirements to ISMS scope and controls. Establish communication channels with key stakeholders.', 'Governance & Leadership', 'high', 2, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '4.3', 'Determining the scope of the information security management system', 'The organization shall determine the boundaries and applicability of the information security management system to establish its scope. When determining this scope, the organization shall consider: a) the external and internal issues referred to in 4.1; b) the requirements referred to in 4.2; c) interfaces and dependencies between activities performed by the organization, and those that are performed by other organizations. The scope shall be available as documented information. Implementation: Define physical and logical boundaries of the ISMS. Include all relevant business processes, locations, and assets. Document interfaces with external organizations. Ensure scope statement is accessible to all stakeholders. Review scope when organizational changes occur.', 'Governance & Leadership', 'high', 3, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '4.4', 'Information security management system', 'The organization shall establish, implement, maintain and continually improve an information security management system, including the processes needed and their interactions, in accordance with the requirements of this document. Implementation: Establish ISMS documentation framework. Define process interactions and responsibilities. Implement monitoring and measurement processes. Create continual improvement procedures. Ensure ISMS alignment with business objectives.', 'Governance & Leadership', 'high', 4, NOW(), NOW(), true);"}	insert_real_iso27001_requirements_v2	payam.razifar@gmail.com	\N
20250701081941	{"-- Insert real ISO 27002 requirements\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- ISO 27002 A.5 Organizational controls\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.1', 'Policies for information security', 'Information security policy and topic-specific policies shall be defined, approved by management, published, communicated to and acknowledged by relevant personnel and relevant interested parties, and reviewed at planned intervals and if significant changes occur. Implementation: Define and document comprehensive information security policies approved by management. Ensure policies are communicated to all employees and relevant external parties. Implement policy acknowledgment process for all personnel. Review policies regularly and update when significant changes occur. Maintain version control and approval records for all policies.', 'Governance & Leadership', 'high', 1, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.2', 'Information security roles and responsibilities', 'Information security roles and responsibilities shall be defined and allocated according to the organization needs. Implementation: Document all information security roles and responsibilities. Assign specific security responsibilities to appropriate personnel. Ensure role clarity and avoid conflicts of interest. Regular review and update of role assignments. Communicate role changes to all affected parties.', 'Governance & Leadership', 'high', 2, NOW(), NOW(), true),\n\n-- ISO 27002 A.8 Asset management  \n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.8.1', 'Inventory of assets', 'Assets associated with information and information processing facilities shall be identified and an inventory of these assets shall be drawn up and maintained. Implementation: Identify all information assets including data, software, hardware, documentation, personnel, and intangible assets. Create and maintain an asset register with ownership, classification, and value. Review asset inventory regularly and update when changes occur.', 'Asset Management', 'high', 3, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.8.2', 'Ownership of assets', 'Assets maintained in the inventory shall be owned. Implementation: Assign ownership for all assets in the inventory. Ensure asset owners understand their responsibilities. Document ownership changes when personnel move or leave. Include ownership information in asset management procedures.', 'Asset Management', 'high', 4, NOW(), NOW(), true);"}	insert_real_iso27002_requirements	payam.razifar@gmail.com	\N
20250701082008	{"-- Insert real CIS Controls requirements\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- CIS Control 1: Inventory and Control of Enterprise Assets\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '1.1', 'Establish and Maintain Detailed Enterprise Asset Inventory', 'Establish and maintain an accurate, detailed, and up-to-date inventory of all enterprise assets with the potential to store or process data, to include: end-user devices (including portable and mobile), network devices, non-computing/IoT devices, and servers. Ensure the inventory records the network address (if static), hardware address, machine name, enterprise asset owner, department for each asset, and whether the asset has been approved to connect to the network. For mobile end-user devices, MDM type tools can support this process, where appropriate. Implementation: Deploy automated asset discovery tools (Lansweeper, ManageEngine, etc.). Include all device types: endpoints, servers, network devices, IoT devices. Record required attributes: IP, MAC, hostname, owner, department, approval status. Establish regular review cycle (bi-annual minimum). Integrate with MDM for mobile device management. Include cloud and virtual assets in inventory scope.', 'Asset Management', 'high', 1, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '1.2', 'Address Unauthorized Assets', 'Ensure that a process exists to address unauthorized assets on a weekly basis. The enterprise may choose to remove the asset from the network, deny the asset from connecting remotely to the network, or quarantine the asset. Implementation: Configure network access control (NAC) solutions. Implement automated quarantine for unknown devices. Establish weekly review process for unauthorized assets. Define escalation procedures for persistent unauthorized assets. Document approved response actions (remove, deny, quarantine). Integrate with incident response procedures.', 'Asset Management', 'high', 2, NOW(), NOW(), true),\n\n-- CIS Control 2: Inventory and Control of Software Assets\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '2.1', 'Establish and Maintain a Software Inventory', 'Establish and maintain a detailed inventory of all licensed software installed on enterprise assets. The software inventory must document the title, publisher, initial install/use date, and business purpose for each entry; where appropriate, include the Uniform Resource Locator (URL), app store(s), version(s), deployment mechanism, and decommission date. Implementation: Deploy software inventory tools to automatically discover installed software. Include operating systems, applications, browser plugins, and cloud services. Document software licensing information and business justification. Establish approval process for new software installations. Regular review and validation of software inventory.', 'Asset Management', 'high', 3, NOW(), NOW(), true),\n\n-- CIS Control 3: Data Protection\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.1', 'Establish and Maintain a Data Management Process', 'Establish and maintain a data management process. In the process, address data sensitivity, data owner, handling of data, data retention limits, and disposal requirements, based on sensitivity and retention standards for the enterprise. Implementation: Classify data based on sensitivity and business value. Assign data owners and define their responsibilities. Establish data handling procedures for different classification levels. Define data retention and disposal requirements. Document data flows and processing activities.', 'Data Protection', 'high', 4, NOW(), NOW(), true);"}	insert_real_cis_controls_requirements	payam.razifar@gmail.com	\N
20250701082126	{"-- Create mappings between framework requirements and unified requirements\n\n-- First, let's map some governance requirements directly\nINSERT INTO unified_requirement_mappings (id, unified_requirement_id, requirement_id, mapping_strength, notes, created_at)\nVALUES\n-- Map ISO 27001 4.1 to Governance & Leadership\n(uuid_generate_v4(), \n (SELECT ur.id FROM unified_requirements ur JOIN unified_compliance_categories uc ON ur.category_id = uc.id WHERE uc.name = 'Governance & Leadership' LIMIT 1),\n (SELECT id FROM requirements_library WHERE control_id = '4.1' AND standard_id = (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001')),\n 'strong', 'ISO 27001 organizational context maps to governance', NOW()),\n\n-- Map ISO 27002 A.5.1 to Governance & Leadership\n(uuid_generate_v4(), \n (SELECT ur.id FROM unified_requirements ur JOIN unified_compliance_categories uc ON ur.category_id = uc.id WHERE uc.name = 'Governance & Leadership' LIMIT 1),\n (SELECT id FROM requirements_library WHERE control_id = 'A.5.1' AND standard_id = (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002')),\n 'exact', 'ISO 27002 information security policies map to governance', NOW()),\n\n-- Map CIS Control 1.1 to Hardware Asset Management\n(uuid_generate_v4(), \n (SELECT ur.id FROM unified_requirements ur JOIN unified_compliance_categories uc ON ur.category_id = uc.id WHERE uc.name = 'Inventory and Control of Hardware Assets' LIMIT 1),\n (SELECT id FROM requirements_library WHERE control_id = '1.1'),\n 'exact', 'CIS Control 1.1 asset inventory maps to hardware asset management', NOW()),\n\n-- Map CIS Control 2.1 to Software Asset Management  \n(uuid_generate_v4(), \n (SELECT ur.id FROM unified_requirements ur JOIN unified_compliance_categories uc ON ur.category_id = uc.id WHERE uc.name = 'Inventory and Control of Software Assets' LIMIT 1),\n (SELECT id FROM requirements_library WHERE control_id = '2.1'),\n 'exact', 'CIS Control 2.1 software inventory maps to software asset management', NOW()),\n\n-- Map CIS Control 3.1 to Data Protection\n(uuid_generate_v4(), \n (SELECT ur.id FROM unified_requirements ur JOIN unified_compliance_categories uc ON ur.category_id = uc.id WHERE uc.name = 'Data Protection' LIMIT 1),\n (SELECT id FROM requirements_library WHERE control_id = '3.1'),\n 'exact', 'CIS Control 3.1 data management maps to data protection', NOW());"}	create_requirement_mappings_v2	payam.razifar@gmail.com	\N
20250701082708	{"-- Clear existing data to ensure clean state\nDELETE FROM unified_requirement_mappings;\nDELETE FROM unified_requirements;\n\n-- Insert unified requirements for Governance & Leadership category\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Governance & Leadership'),\n    'Information Security Governance & Leadership',\n    'Comprehensive governance framework with leadership commitment and organizational structure',\n    ARRAY[\n        'a) UNIFIED POLICY FRAMEWORK: Establish comprehensive information security policy that satisfies ISO 27001 leadership requirements, ISO 27002 policy controls, GDPR data protection principles, and NIS2 governance measures - ensuring top management commitment and regulatory compliance across all frameworks',\n        'b) UNIFIED GOVERNANCE STRUCTURE: Define integrated governance with clear roles, responsibilities, and authorities that address ISO 27001 ISMS scope, ISO 27002 organizational controls, GDPR controller/processor responsibilities, and NIS2 cybersecurity governance requirements including management approval of cybersecurity risk policies (Art. 21(2)(a)), management responsibilities for implementation (Art. 21(2)(f)), and board oversight of cybersecurity strategy',\n        'c) UNIFIED RISK MANAGEMENT: Implement consolidated risk assessment and treatment processes that satisfy ISO 27001 risk management (clauses 6.1.2, 6.1.3, 8.2, 8.3), ISO 27002 threat intelligence collection (A.5.7), GDPR data protection impact assessments (Art. 35, 36), and NIS2 cybersecurity risk management measures including risk analysis policies (Art. 21(2)(a)) and effectiveness assessment procedures (Art. 21(2)(g))',\n        'd) UNIFIED COMPLIANCE MONITORING: Establish integrated audit, monitoring, and review processes that meet ISO 27001 internal audit requirements, ISO 27002 independent reviews, GDPR compliance monitoring, and NIS2 effectiveness assessment obligations',\n        'e) UNIFIED DOCUMENTATION SYSTEM: Maintain comprehensive documentation framework that satisfies ISO 27001 documented information requirements, ISO 27002 operating procedures, GDPR records of processing, and NIS2 policy documentation mandates',\n        'f) UNIFIED STAKEHOLDER ENGAGEMENT: Establish consolidated communication with authorities, special interest groups, and relevant parties as required by ISO 27002 contacts, GDPR supervisory authority relations, and NIS2 incident reporting obligations',\n        'g) UNIFIED CONTINUOUS IMPROVEMENT: Implement integrated improvement processes that address ISO 27001 continual improvement, ISO 27002 learning from incidents, GDPR regular review requirements, and NIS2 effectiveness evaluation measures',\n        'h) SCREENING: Conduct background verification checks on candidates for employment, contractors, and third-party users in accordance with applicable laws, regulations, and ethical considerations, proportional to business requirements, data classification, and perceived risks'\n    ],\n    1\n);"}	apply_complete_unified_compliance_data	payam.razifar@gmail.com	\N
20250701082737	{"-- Add Risk Management unified requirement\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Risk Management'),\n    'Information Security Risk Management',\n    'Comprehensive risk assessment, treatment, and monitoring framework',\n    ARRAY[\n        'a) UNIFIED RISK IDENTIFICATION: Establish comprehensive risk assessment process that integrates GDPR data processing risk identification and NIS2 cybersecurity threat analysis into one systematic approach',\n        'b) UNIFIED RISK EVALUATION: Implement consolidated risk analysis methodology that addresses both quantitative and qualitative assessment criteria, satisfying GDPR data protection impact assessment requirements and NIS2 cybersecurity risk evaluation measures',\n        'c) UNIFIED RISK TREATMENT: Develop integrated risk treatment strategy that implements appropriate technical and organizational measures satisfying ISO 27002 controls, GDPR Article 32 security requirements, and NIS2 cybersecurity risk management measures',\n        'd) UNIFIED RISK MONITORING: Establish continuous risk monitoring and effectiveness assessment process that meets NIS2 Article 21(2)(g) effectiveness evaluation requirements and GDPR regular review obligations'\n    ],\n    1\n);\n\n-- Add Asset Management unified requirements\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Inventory and Control of Hardware Assets'),\n    'Hardware Asset Inventory and Control',\n    'Comprehensive hardware asset management including inventory, tracking, and lifecycle management',\n    ARRAY[\n        'a) Maintain accurate and detailed inventory of all hardware assets',\n        'b) Address unauthorized hardware assets within defined timeframes',\n        'c) Utilize automated asset discovery and inventory tools',\n        'd) Use technical controls to prevent unauthorized connections',\n        'e) Manage asset lifecycle from procurement to disposal',\n        'f) Implement asset classification and labeling',\n        'g) Establish asset ownership and responsibility',\n        'h) Monitor and audit asset inventory regularly'\n    ],\n    1\n);\n\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Inventory and Control of Software Assets'),\n    'Software Asset Inventory and Control',\n    'Comprehensive software asset management including inventory, licensing, and allowlisting',\n    ARRAY[\n        'a) Maintain accurate and detailed inventory of all software assets',\n        'b) Address unauthorized software within defined timeframes',\n        'c) Utilize automated software inventory tools and processes',\n        'd) Use technical controls to enforce software allowlisting',\n        'e) Allowlist authorized software libraries and dependencies',\n        'f) Allowlist authorized scripts and executables',\n        'g) Control installation of software on operational systems',\n        'h) Manage software licensing and compliance'\n    ],\n    1\n);\n\n-- Add Data Protection unified requirement\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Data Protection'),\n    'Data Protection & Cryptographic Controls',\n    'Comprehensive data protection including classification, handling, encryption, and privacy',\n    ARRAY[\n        'a) UNIFIED DATA CLASSIFICATION: Establish comprehensive data classification that satisfies ISO 27001 asset management, ISO 27002 information classification, GDPR data categorization (personal, special categories), and NIS2 data criticality assessment',\n        'b) UNIFIED DATA HANDLING: Implement secure data handling procedures that address ISO 27002 secure handling requirements, GDPR lawful basis and consent management, and NIS2 data integrity measures',\n        'c) UNIFIED ENCRYPTION STRATEGY: Deploy encryption controls that meet ISO 27002 cryptographic requirements, GDPR Article 32 security measures, and NIS2 cybersecurity measures for data protection',\n        'd) UNIFIED PRIVACY CONTROLS: Establish privacy protection measures that satisfy GDPR rights management (Articles 15-22), data minimization principles, and data subject request procedures',\n        'e) UNIFIED DATA RETENTION: Implement data retention and disposal policies that comply with GDPR retention limitations and deletion requirements',\n        'f) UNIFIED DATA TRANSFER: Establish secure data transfer mechanisms that meet GDPR international transfer requirements and NIS2 supply chain security measures'\n    ],\n    1\n);"}	add_more_unified_requirements	payam.razifar@gmail.com	\N
20250701082806	{"-- Delete existing mappings to recreate them properly\nDELETE FROM unified_requirement_mappings;\n\n-- Map ISO 27001 requirements to Governance & Leadership\nINSERT INTO unified_requirement_mappings (id, unified_requirement_id, requirement_id, mapping_strength, notes, created_at)\nSELECT \n    uuid_generate_v4(),\n    ur.id,\n    r.id,\n    'strong',\n    'ISO 27001 organizational context requirements map to governance framework',\n    NOW()\nFROM unified_requirements ur\nJOIN unified_compliance_categories uc ON ur.category_id = uc.id\nJOIN requirements_library r ON r.control_id IN ('4.1', '4.2', '4.3', '4.4')\nJOIN standards_library s ON r.standard_id = s.id\nWHERE uc.name = 'Governance & Leadership' \n  AND s.name = 'ISO/IEC 27001'\n  AND ur.title = 'Information Security Governance & Leadership';\n\n-- Map ISO 27002 A.5 requirements to Governance & Leadership\nINSERT INTO unified_requirement_mappings (id, unified_requirement_id, requirement_id, mapping_strength, notes, created_at)\nSELECT \n    uuid_generate_v4(),\n    ur.id,\n    r.id,\n    'exact',\n    'ISO 27002 organizational controls map directly to governance',\n    NOW()\nFROM unified_requirements ur\nJOIN unified_compliance_categories uc ON ur.category_id = uc.id\nJOIN requirements_library r ON r.control_id IN ('A.5.1', 'A.5.2')\nJOIN standards_library s ON r.standard_id = s.id\nWHERE uc.name = 'Governance & Leadership' \n  AND s.name = 'ISO/IEC 27002'\n  AND ur.title = 'Information Security Governance & Leadership';\n\n-- Map CIS Control 1.x to Hardware Asset Management\nINSERT INTO unified_requirement_mappings (id, unified_requirement_id, requirement_id, mapping_strength, notes, created_at)\nSELECT \n    uuid_generate_v4(),\n    ur.id,\n    r.id,\n    'exact',\n    'CIS Control 1.x maps to hardware asset inventory and control',\n    NOW()\nFROM unified_requirements ur\nJOIN unified_compliance_categories uc ON ur.category_id = uc.id\nJOIN requirements_library r ON r.control_id LIKE '1.%'\nJOIN standards_library s ON r.standard_id = s.id\nWHERE uc.name = 'Inventory and Control of Hardware Assets' \n  AND s.name = 'CIS Controls Implementation Group 3 (IG3)'\n  AND ur.title = 'Hardware Asset Inventory and Control';\n\n-- Map CIS Control 2.x to Software Asset Management\nINSERT INTO unified_requirement_mappings (id, unified_requirement_id, requirement_id, mapping_strength, notes, created_at)\nSELECT \n    uuid_generate_v4(),\n    ur.id,\n    r.id,\n    'exact',\n    'CIS Control 2.x maps to software asset inventory and control',\n    NOW()\nFROM unified_requirements ur\nJOIN unified_compliance_categories uc ON ur.category_id = uc.id\nJOIN requirements_library r ON r.control_id LIKE '2.%'\nJOIN standards_library s ON r.standard_id = s.id\nWHERE uc.name = 'Inventory and Control of Software Assets' \n  AND s.name = 'CIS Controls Implementation Group 3 (IG3)'\n  AND ur.title = 'Software Asset Inventory and Control';\n\n-- Map CIS Control 3.x to Data Protection\nINSERT INTO unified_requirement_mappings (id, unified_requirement_id, requirement_id, mapping_strength, notes, created_at)\nSELECT \n    uuid_generate_v4(),\n    ur.id,\n    r.id,\n    'exact',\n    'CIS Control 3.x maps to data protection and management',\n    NOW()\nFROM unified_requirements ur\nJOIN unified_compliance_categories uc ON ur.category_id = uc.id\nJOIN requirements_library r ON r.control_id LIKE '3.%'\nJOIN standards_library s ON r.standard_id = s.id\nWHERE uc.name = 'Data Protection' \n  AND s.name = 'CIS Controls Implementation Group 3 (IG3)'\n  AND ur.title = 'Data Protection & Cryptographic Controls';\n\n-- Map ISO 27002 A.8 requirements to Hardware Asset Management  \nINSERT INTO unified_requirement_mappings (id, unified_requirement_id, requirement_id, mapping_strength, notes, created_at)\nSELECT \n    uuid_generate_v4(),\n    ur.id,\n    r.id,\n    'exact',\n    'ISO 27002 A.8 asset management controls map to hardware assets',\n    NOW()\nFROM unified_requirements ur\nJOIN unified_compliance_categories uc ON ur.category_id = uc.id\nJOIN requirements_library r ON r.control_id LIKE 'A.8.%'\nJOIN standards_library s ON r.standard_id = s.id\nWHERE uc.name = 'Inventory and Control of Hardware Assets' \n  AND s.name = 'ISO/IEC 27002'\n  AND ur.title = 'Hardware Asset Inventory and Control';"}	create_comprehensive_mappings	payam.razifar@gmail.com	\N
20250701082850	{"-- Add comprehensive ISO 27001 requirements (all major clauses)\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- ISO 27001 Section 5 Leadership\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '5.1', 'Leadership and commitment', 'Top management shall demonstrate leadership and commitment with respect to the information security management system by ensuring the information security policy and the information security objectives are established and are compatible with the strategic direction of the organization. Implementation: Establish clear leadership commitment to information security. Ensure information security policy aligns with business strategy. Allocate adequate resources for ISMS implementation and maintenance. Demonstrate visible support for information security initiatives.', 'Governance & Leadership', 'high', 5, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '5.2', 'Policy', 'Top management shall establish an information security policy that includes a framework for setting information security objectives and includes a commitment to satisfy applicable requirements related to information security. Implementation: Develop comprehensive information security policy approved by top management. Include framework for setting security objectives. Ensure policy addresses all applicable legal and regulatory requirements. Communicate policy to all relevant parties.', 'Governance & Leadership', 'high', 6, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '5.3', 'Organizational roles, responsibilities and authorities', 'Top management shall ensure that the responsibilities and authorities for roles relevant to information security are assigned and communicated. Implementation: Define and document all information security roles and responsibilities. Assign specific authorities for information security decisions. Communicate role assignments to all relevant personnel. Establish clear reporting lines for information security matters.', 'Governance & Leadership', 'high', 7, NOW(), NOW(), true),\n\n-- ISO 27001 Section 6 Planning\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '6.1.1', 'General actions to address risks and opportunities', 'When planning for the information security management system, the organization shall consider the issues referred to in 4.1 and the requirements referred to in 4.2 and determine the risks and opportunities that need to be addressed. Implementation: Identify all risks and opportunities related to information security. Consider internal and external context factors. Address stakeholder requirements and expectations. Document risk and opportunity assessment process.', 'Risk Management', 'high', 8, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '6.1.2', 'Information security risk assessment', 'The organization shall define and apply an information security risk assessment process that establishes and maintains information security risk criteria and consistently produces comparable and reproducible results. Implementation: Establish formal risk assessment methodology. Define risk acceptance criteria and risk evaluation criteria. Ensure consistent application across all assessments. Document risk assessment process and results.', 'Risk Management', 'high', 9, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '6.1.3', 'Information security risk treatment', 'The organization shall define and apply an information security risk treatment process to select appropriate information security risk treatment options and determine all controls that are necessary to implement the information security risk treatment option(s) chosen. Implementation: Define risk treatment options (accept, avoid, transfer, mitigate). Select appropriate controls for risk treatment. Document risk treatment decisions and rationale. Implement selected controls systematically.', 'Risk Management', 'high', 10, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '6.2', 'Information security objectives and planning to achieve them', 'The organization shall establish information security objectives at relevant functions and levels. Information security objectives shall be consistent with the information security policy. Implementation: Set measurable information security objectives. Align objectives with organizational strategy and policy. Assign responsibility for achieving objectives. Monitor and measure progress toward objectives.', 'Governance & Leadership', 'high', 11, NOW(), NOW(), true),\n\n-- ISO 27001 Section 7 Support\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '7.1', 'Resources', 'The organization shall determine and provide the resources needed for the establishment, implementation, maintenance and continual improvement of the information security management system. Implementation: Identify all resource requirements for ISMS. Allocate adequate human, financial, and technical resources. Ensure resources are available when needed. Review resource allocation regularly.', 'Governance & Leadership', 'medium', 12, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '7.2', 'Competence', 'The organization shall determine the necessary competence of person(s) doing work under its control that affects information security performance, ensure that these persons are competent on the basis of appropriate education, training, or experience. Implementation: Define competency requirements for information security roles. Assess current competency levels. Provide training and development programs. Maintain records of competency and training.', 'Security Awareness & Skills Training', 'high', 13, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '7.3', 'Awareness', 'Persons doing work under the organization control shall be aware of the information security policy, their contribution to the effectiveness of the information security management system, and the implications of not conforming with the information security management system requirements. Implementation: Develop comprehensive awareness program. Communicate information security policy to all personnel. Ensure understanding of individual roles in information security. Provide regular awareness updates and refreshers.', 'Security Awareness & Skills Training', 'high', 14, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '7.4', 'Communication', 'The organization shall determine the need for internal and external communications relevant to the information security management system including on what to communicate, when to communicate, with whom to communicate, who shall communicate, and the processes by which communication shall be effected. Implementation: Establish communication protocols for information security matters. Define internal and external communication requirements. Assign communication responsibilities. Implement effective communication channels and methods.', 'Governance & Leadership', 'medium', 15, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '7.5', 'Documented information', 'The information security management system shall include documented information required by this document and documented information determined by the organization as being necessary for the effectiveness of the information security management system. Implementation: Maintain comprehensive documentation for ISMS. Ensure documented information is current and accurate. Control creation, update, and distribution of documents. Implement document retention and disposal procedures.', 'Governance & Leadership', 'medium', 16, NOW(), NOW(), true);"}	add_comprehensive_iso27001_requirements	payam.razifar@gmail.com	\N
20250701082931	{"-- Add comprehensive ISO 27002 controls across all categories\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- A.5 Organizational controls (continuing from A.5.1, A.5.2)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.3', 'Segregation of duties', 'Conflicting duties and areas of responsibility shall be segregated to reduce opportunities for unauthorized or unintentional modification or misuse of the organization information. Implementation: Identify conflicting duties that could lead to fraud or error. Separate responsibilities for authorization, recording, and custody. Implement approval workflows that prevent single-person authorization. Document segregation of duties matrix.', 'Governance & Leadership', 'high', 20, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.4', 'Management responsibilities', 'Management shall require all personnel to apply information security in accordance with the established policies and procedures of the organization. Implementation: Establish clear management accountability for information security. Define management responsibilities at all levels. Implement regular review of security performance. Ensure management oversight of security controls implementation.', 'Governance & Leadership', 'high', 21, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.5', 'Contact with authorities', 'Appropriate contacts with relevant authorities shall be maintained. Implementation: Identify relevant regulatory and law enforcement authorities. Establish formal contact procedures. Maintain current contact information. Define escalation procedures for authority involvement.', 'Governance & Leadership', 'medium', 22, NOW(), NOW(), true),\n\n-- A.6 People controls  \n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.6.1', 'Screening', 'Background verification checks on all candidates for employment shall be carried out in accordance with relevant laws, regulations and ethics and shall be proportional to the business requirements, the classification of the information to be accessed and the perceived risks. Implementation: Conduct background checks proportional to role sensitivity. Verify employment history, education, and references. Check for criminal history where legally permitted. Document screening procedures and results.', 'Governance & Leadership', 'high', 23, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.6.2', 'Terms and conditions of employment', 'The contractual agreements with personnel and contractors shall state their and the organization responsibilities for information security. Implementation: Include information security clauses in employment contracts. Define confidentiality and non-disclosure requirements. Specify acceptable use of information systems. Include consequences for security violations.', 'Governance & Leadership', 'high', 24, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.6.3', 'Information security awareness, education and training', 'All personnel of the organization and, where relevant, contractors shall receive appropriate awareness education and training and regular updates in organizational policies and procedures, as relevant for their job function. Implementation: Develop comprehensive security awareness program. Provide role-specific security training. Conduct regular security awareness sessions. Measure effectiveness of training programs.', 'Security Awareness & Skills Training', 'high', 25, NOW(), NOW(), true),\n\n-- A.7 Physical and environmental security\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.1', 'Physical security perimeters', 'Physical security perimeters shall be defined and used to protect areas that contain either sensitive or critical information and information processing facilities. Implementation: Define security perimeters for sensitive areas. Implement physical barriers and access controls. Install monitoring and detection systems. Control access points and entry procedures.', 'Physical & Environmental Security Controls', 'high', 26, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.2', 'Physical entry', 'Secure areas shall be protected by appropriate entry controls to ensure that only authorized personnel are allowed access. Implementation: Implement access control systems for secure areas. Require authorization for entry to sensitive areas. Maintain visitor access logs and escort procedures. Monitor and record all access to secure areas.', 'Physical & Environmental Security Controls', 'high', 27, NOW(), NOW(), true),\n\n-- A.8 Asset management (continuing from A.8.1, A.8.2)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.8.3', 'Handling of assets', 'Procedures for handling assets shall be developed and implemented in accordance with the information classification scheme adopted by the organization. Implementation: Develop asset handling procedures based on classification. Define secure transport and storage requirements. Implement secure disposal procedures. Train personnel on proper asset handling.', 'Asset Management', 'high', 28, NOW(), NOW(), true),\n\n-- A.9 Access control\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.9.1', 'Access control policy', 'An access control policy shall be established, documented and reviewed based on business and information security requirements. Implementation: Develop comprehensive access control policy. Align with business requirements and risk assessment. Regular review and update of access control policies. Communicate policy to all relevant stakeholders.', 'Identity & Access Management', 'high', 29, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.9.2', 'Access to networks and network services', 'Users shall only be provided with access to the network and network services that they have been specifically authorized to use. Implementation: Implement network access controls based on authorization. Restrict access to network services by role and need. Monitor network access and usage. Regularly review and validate network access permissions.', 'Identity & Access Management', 'high', 30, NOW(), NOW(), true),\n\n-- A.10 Cryptography\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.10.1', 'Cryptographic controls', 'A policy on the use of cryptographic controls for protection of information shall be developed and implemented. Implementation: Develop cryptographic policy covering encryption requirements. Implement appropriate cryptographic controls for data protection. Manage cryptographic keys securely. Regular review of cryptographic implementations.', 'Data Protection', 'high', 31, NOW(), NOW(), true);"}	add_comprehensive_iso27002_requirements	payam.razifar@gmail.com	\N
20250701083017	{"-- Add comprehensive CIS Controls across all 18 control families\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- CIS Control 1: Additional inventory controls\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '1.3', 'Use DHCP Logging to Update Asset Inventory', 'Use Dynamic Host Configuration Protocol (DHCP) logging on all DHCP servers or Internet Protocol (IP) address management tools to update the enterprise asset inventory. Implementation: Configure DHCP servers to log all IP address assignments. Integrate DHCP logs with asset inventory systems. Monitor for unknown devices requesting IP addresses. Automate inventory updates from DHCP data.', 'Inventory and Control of Hardware Assets', 'medium', 35, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '1.4', 'Use Automated Asset Discovery Tools', 'Use automated discovery tools to identify assets connected to the enterprise network and update the hardware asset inventory. Implementation: Deploy automated asset discovery solutions. Schedule regular network scans for asset identification. Configure discovery tools to detect various device types. Integrate discovery results with central asset inventory.', 'Inventory and Control of Hardware Assets', 'high', 36, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '1.5', 'Use a Passive Asset Discovery Tool', 'Use a passive discovery tool to identify assets connected to the enterprise network. Implementation: Deploy passive network monitoring tools. Analyze network traffic to identify connected devices. Detect assets without active scanning. Maintain continuous visibility of network-connected assets.', 'Inventory and Control of Hardware Assets', 'medium', 37, NOW(), NOW(), true),\n\n-- CIS Control 2: Additional software controls\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '2.2', 'Ensure Authorized Software is Currently Supported', 'Ensure that only currently supported software is designated as authorized in the software inventory for enterprise assets. Implementation: Maintain list of supported software versions. Remove unsupported software from authorized lists. Implement upgrade plans for end-of-life software. Monitor vendor support status for all authorized software.', 'Inventory and Control of Software Assets', 'high', 38, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '2.3', 'Address Unauthorized Software', 'Ensure that unauthorized software is either removed from use on enterprise assets or receives a documented exception. Implementation: Identify and remove unauthorized software installations. Document exceptions for business-critical unauthorized software. Implement technical controls to prevent unauthorized software execution. Regular audits of software compliance.', 'Inventory and Control of Software Assets', 'high', 39, NOW(), NOW(), true),\n\n-- CIS Control 4: Secure Configuration\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '4.1', 'Establish and Maintain a Secure Configuration Process', 'Establish and maintain a secure configuration process for enterprise assets (end-user devices, including portable and mobile, non-computing/IoT devices, and servers) and software (operating systems and applications). Implementation: Develop secure configuration standards for all asset types. Implement configuration management processes. Document approved configurations and settings. Regular review and update of configuration standards.', 'Secure Configuration of Hardware and Software', 'high', 40, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '4.2', 'Establish and Maintain a Secure Configuration Process for Network Infrastructure', 'Establish and maintain a secure configuration process for network devices. Implementation: Develop network device configuration standards. Implement secure configuration baselines for routers, switches, firewalls. Document network configuration requirements. Regular auditing of network device configurations.', 'Secure Configuration of Hardware and Software', 'high', 41, NOW(), NOW(), true),\n\n-- CIS Control 5: Account Management\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '5.1', 'Establish and Maintain an Inventory of Accounts', 'Establish and maintain an inventory of all accounts managed in the enterprise. The inventory shall include both user and administrator accounts. Implementation: Maintain comprehensive account inventory including all user types. Document account types, purposes, and owners. Regular review and validation of account inventory. Implement automated account discovery where possible.', 'Identity & Access Management', 'high', 42, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '5.2', 'Use Unique Passwords', 'Use unique passwords for all enterprise assets. Default passwords shall be changed or the asset shall be removed from the network. Implementation: Ensure all accounts have unique, non-default passwords. Implement password complexity requirements. Prohibit password reuse across multiple accounts. Regular auditing for default or weak passwords.', 'Identity & Access Management', 'high', 43, NOW(), NOW(), true),\n\n-- CIS Control 6: Access Control Management\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '6.1', 'Establish an Access Granting Process', 'Establish and follow a process, preferably automated, for granting access to enterprise assets upon new hire, promotion, or role change of a user. Implementation: Define formal access request and approval process. Implement role-based access control principles. Document access granting procedures and approval workflows. Automate access provisioning where possible.', 'Identity & Access Management', 'high', 44, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '6.2', 'Establish an Access Revoking Process', 'Establish and follow a process, preferably automated, for revoking access to enterprise assets, through disabling accounts immediately upon termination, role change of a user. Implementation: Define formal access revocation process for terminations and role changes. Implement automated account disabling procedures. Document access removal timelines and procedures. Regular audit of access revocation compliance.', 'Identity & Access Management', 'high', 45, NOW(), NOW(), true),\n\n-- CIS Control 7: Continuous Vulnerability Management\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '7.1', 'Establish and Maintain a Vulnerability Management Process', 'Establish and maintain a vulnerability management process to identify and address vulnerabilities. Implementation: Define vulnerability management lifecycle and procedures. Implement vulnerability scanning and assessment tools. Establish vulnerability prioritization and remediation processes. Document vulnerability management workflows and responsibilities.', 'Vulnerability Management', 'high', 46, NOW(), NOW(), true),\n\n-- CIS Control 8: Audit Log Management\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '8.1', 'Establish and Maintain an Audit Log Management Process', 'Establish and maintain an audit log management process that defines the enterprise logging requirements. Implementation: Define comprehensive logging requirements for all systems. Establish log collection, storage, and retention procedures. Implement centralized log management solutions. Document audit log management processes and procedures.', 'Audit Log Management', 'high', 47, NOW(), NOW(), true);"}	add_comprehensive_cis_controls	payam.razifar@gmail.com	\N
20250701083214	{"-- Add unified requirements for categories that are missing them\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order) VALUES\n\n-- Identity & Access Management\n((SELECT id FROM unified_compliance_categories WHERE name = 'Identity & Access Management'),\n'Identity and Access Management Controls',\n'Comprehensive identity lifecycle management, authentication, and authorization controls',\nARRAY[\n    'a) Identity Lifecycle Management: Implement comprehensive identity management covering account provisioning, modification, and deprovisioning',\n    'b) Authentication Controls: Establish strong authentication mechanisms including multi-factor authentication',\n    'c) Authorization Management: Implement role-based access control and principle of least privilege',\n    'd) Privileged Access Management: Control and monitor privileged accounts and access'\n],\n1),\n\n-- Secure Configuration\n((SELECT id FROM unified_compliance_categories WHERE name = 'Secure Configuration of Hardware and Software'),\n'Secure Configuration Management',\n'Enterprise-wide secure configuration management for all assets and systems',\nARRAY[\n    'a) Configuration Standards: Establish and maintain secure configuration baselines',\n    'b) Configuration Management: Implement configuration control and change management',\n    'c) Configuration Monitoring: Monitor and validate configuration compliance',\n    'd) Configuration Documentation: Maintain comprehensive configuration documentation'\n],\n1),\n\n-- Vulnerability Management\n((SELECT id FROM unified_compliance_categories WHERE name = 'Vulnerability Management'),\n'Vulnerability Assessment and Management',\n'Comprehensive vulnerability management including assessment, patching, and remediation',\nARRAY[\n    'a) Vulnerability Assessment: Conduct regular vulnerability scanning and assessment',\n    'b) Vulnerability Prioritization: Prioritize vulnerabilities based on risk and criticality',\n    'c) Patch Management: Implement timely patching and remediation processes',\n    'd) Vulnerability Monitoring: Continuously monitor for new vulnerabilities and threats'\n],\n1),\n\n-- Physical Security\n((SELECT id FROM unified_compliance_categories WHERE name = 'Physical & Environmental Security Controls'),\n'Physical and Environmental Security',\n'Comprehensive physical security covering facilities, equipment, and environmental controls',\nARRAY[\n    'a) Physical Access Controls: Implement physical access controls and monitoring',\n    'b) Environmental Controls: Maintain appropriate environmental conditions',\n    'c) Equipment Security: Secure and monitor physical equipment and media',\n    'd) Facility Security: Establish secure perimeters and physical barriers'\n],\n1),\n\n-- Security Awareness\n((SELECT id FROM unified_compliance_categories WHERE name = 'Security Awareness & Skills Training'),\n'Security Awareness and Training',\n'Comprehensive security awareness and training program covering all aspects of cybersecurity education',\nARRAY[\n    'a) Security Awareness Program: Establish comprehensive security awareness program',\n    'b) Role-based Training: Provide role-specific security training and education',\n    'c) Training Effectiveness: Measure and improve training program effectiveness',\n    'd) Ongoing Education: Maintain continuous security education and updates'\n],\n1),\n\n-- Audit Logging\n((SELECT id FROM unified_compliance_categories WHERE name = 'Audit Log Management'),\n'Audit Log Collection and Management',\n'Comprehensive audit log collection, storage, and monitoring for security and compliance',\nARRAY[\n    'a) Log Collection: Collect comprehensive audit logs from all systems',\n    'b) Log Storage: Securely store and retain audit logs according to requirements',\n    'c) Log Monitoring: Monitor audit logs for security events and incidents',\n    'd) Log Analysis: Analyze logs for compliance and security purposes'\n],\n1);"}	add_missing_unified_requirements	payam.razifar@gmail.com	\N
20250701083235	{"-- Map existing requirements to the new unified requirements\n\n-- Map ISO 27001 training requirements to Security Awareness\nINSERT INTO unified_requirement_mappings (id, unified_requirement_id, requirement_id, mapping_strength, notes, created_at)\nSELECT \n    uuid_generate_v4(),\n    ur.id,\n    r.id,\n    'exact',\n    'ISO 27001 competence and awareness requirements map to training',\n    NOW()\nFROM unified_requirements ur\nJOIN unified_compliance_categories uc ON ur.category_id = uc.id\nJOIN requirements_library r ON r.control_id IN ('7.2', '7.3')\nJOIN standards_library s ON r.standard_id = s.id\nWHERE uc.name = 'Security Awareness & Skills Training' \n  AND s.name = 'ISO/IEC 27001'\n  AND ur.title = 'Security Awareness and Training';\n\n-- Map ISO 27002 A.6.3 to Security Awareness\nINSERT INTO unified_requirement_mappings (id, unified_requirement_id, requirement_id, mapping_strength, notes, created_at)\nSELECT \n    uuid_generate_v4(),\n    ur.id,\n    r.id,\n    'exact',\n    'ISO 27002 security awareness training maps to security awareness',\n    NOW()\nFROM unified_requirements ur\nJOIN unified_compliance_categories uc ON ur.category_id = uc.id\nJOIN requirements_library r ON r.control_id = 'A.6.3'\nJOIN standards_library s ON r.standard_id = s.id\nWHERE uc.name = 'Security Awareness & Skills Training' \n  AND s.name = 'ISO/IEC 27002'\n  AND ur.title = 'Security Awareness and Training';\n\n-- Map ISO 27002 physical security controls\nINSERT INTO unified_requirement_mappings (id, unified_requirement_id, requirement_id, mapping_strength, notes, created_at)\nSELECT \n    uuid_generate_v4(),\n    ur.id,\n    r.id,\n    'exact',\n    'ISO 27002 physical security controls',\n    NOW()\nFROM unified_requirements ur\nJOIN unified_compliance_categories uc ON ur.category_id = uc.id\nJOIN requirements_library r ON r.control_id IN ('A.7.1', 'A.7.2')\nJOIN standards_library s ON r.standard_id = s.id\nWHERE uc.name = 'Physical & Environmental Security Controls' \n  AND s.name = 'ISO/IEC 27002'\n  AND ur.title = 'Physical and Environmental Security';\n\n-- Map ISO 27002 access control to Identity & Access Management\nINSERT INTO unified_requirement_mappings (id, unified_requirement_id, requirement_id, mapping_strength, notes, created_at)\nSELECT \n    uuid_generate_v4(),\n    ur.id,\n    r.id,\n    'exact',\n    'ISO 27002 access control maps to identity and access management',\n    NOW()\nFROM unified_requirements ur\nJOIN unified_compliance_categories uc ON ur.category_id = uc.id\nJOIN requirements_library r ON r.control_id IN ('A.9.1', 'A.9.2')\nJOIN standards_library s ON r.standard_id = s.id\nWHERE uc.name = 'Identity & Access Management' \n  AND s.name = 'ISO/IEC 27002'\n  AND ur.title = 'Identity and Access Management Controls';"}	map_new_unified_requirements	payam.razifar@gmail.com	\N
20250701083645	{"-- Add the remaining unified requirements (continuing from what we have)\n\n-- Network Infrastructure Management\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Network Infrastructure Management'),\n    'Network Infrastructure Security',\n    'Comprehensive network security including infrastructure protection, monitoring, and secure communications',\n    ARRAY[\n        'a) Implement network segmentation and secure network architecture',\n        'b) Configure and maintain network security controls and firewalls',\n        'c) Monitor network traffic and implement intrusion detection',\n        'd) Establish secure remote access and VPN capabilities',\n        'e) Implement wireless network security controls',\n        'f) Maintain network documentation and configuration management',\n        'g) Conduct regular network security assessments',\n        'h) Implement network access control and authentication'\n    ],\n    1\n);\n\n-- Secure Software Development\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Secure Software Development'),\n    'Secure Development Lifecycle',\n    'Comprehensive secure development practices covering SDLC, testing, and deployment',\n    ARRAY[\n        'a) Establish secure development lifecycle (SDLC) processes',\n        'b) Implement secure coding standards and practices',\n        'c) Conduct security code reviews and testing',\n        'd) Implement application security testing (SAST/DAST)',\n        'e) Establish secure deployment and configuration management',\n        'f) Implement dependency management and third-party component security',\n        'g) Conduct penetration testing of applications',\n        'h) Maintain security throughout application lifecycle'\n    ],\n    1\n);\n\n-- Incident Response Management\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Incident Response Management'),\n    'Incident Response and Management',\n    'Comprehensive incident response covering detection, analysis, containment, and recovery',\n    ARRAY[\n        'a) Establish incident response procedures and team',\n        'b) Implement incident detection and monitoring capabilities',\n        'c) Conduct incident analysis and classification',\n        'd) Implement incident containment and eradication procedures',\n        'e) Establish incident recovery and post-incident activities',\n        'f) Maintain incident response documentation and reporting',\n        'g) Conduct incident response training and exercises',\n        'h) Coordinate with external parties during incidents'\n    ],\n    1\n);\n\n-- Network Monitoring & Defense\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Network Monitoring & Defense'),\n    'Network Defense and Monitoring',\n    'Comprehensive network monitoring, intrusion detection, and network defense capabilities',\n    ARRAY[\n        'a) Deploy network monitoring and analysis tools',\n        'b) Implement intrusion detection and prevention systems',\n        'c) Establish network traffic analysis and logging',\n        'd) Implement network-based malware detection',\n        'e) Conduct network security monitoring and incident response',\n        'f) Establish network forensics capabilities',\n        'g) Implement network threat intelligence integration',\n        'h) Maintain network security event correlation'\n    ],\n    1\n);\n\n-- Supplier & Third-Party Risk Management\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Supplier & Third-Party Risk Management'),\n    'Supplier and Third-Party Risk Management',\n    'Comprehensive supplier and third-party risk management including vendor relationships and cloud services',\n    ARRAY[\n        'a) Establish supplier risk assessment and management processes',\n        'b) Implement third-party security requirements and contracts',\n        'c) Conduct vendor security assessments and due diligence',\n        'd) Establish cloud service provider security requirements',\n        'e) Implement supply chain security controls',\n        'f) Monitor third-party security compliance',\n        'g) Establish data sharing agreements and controls',\n        'h) Manage third-party access and privileged accounts'\n    ],\n    1\n);\n\n-- Business Continuity & Disaster Recovery\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Business Continuity & Disaster Recovery Management'),\n    'Business Continuity and Disaster Recovery',\n    'Comprehensive business continuity planning covering disruption management and recovery capabilities',\n    ARRAY[\n        'a) Establish business continuity planning and management',\n        'b) Conduct business impact analysis and risk assessment',\n        'c) Develop disaster recovery procedures and capabilities',\n        'd) Implement data backup and recovery processes',\n        'e) Establish alternative processing facilities',\n        'f) Conduct business continuity testing and exercises',\n        'g) Maintain business continuity documentation',\n        'h) Train personnel on continuity procedures'\n    ],\n    1\n);\n\n-- Malware Defenses\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Malware Defenses'),\n    'Malware Protection and Defense',\n    'Comprehensive malware protection covering detection, prevention, and response',\n    ARRAY[\n        'a) Deploy and maintain anti-malware software on all enterprise assets',\n        'b) Configure anti-malware software to automatically update signatures',\n        'c) Enable real-time scanning and monitoring capabilities',\n        'd) Establish procedures for handling malware incidents',\n        'e) Implement email and web-based malware filtering',\n        'f) Conduct regular malware scans of systems and storage media',\n        'g) Train users on malware identification and reporting',\n        'h) Maintain centralized management of anti-malware solutions'\n    ],\n    1\n);\n\n-- Email & Web Browser Protections\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Email & Web Browser Protections'),\n    'Email and Web Browser Security',\n    'Comprehensive security for web browsing, email communications, and web-based applications',\n    ARRAY[\n        'a) Use only fully supported and updated web browsers and email clients',\n        'b) Implement DNS filtering services for all enterprise assets',\n        'c) Maintain and enforce network-based URL filtering policies',\n        'd) Deploy email security gateways with anti-spam and anti-malware',\n        'e) Implement web application firewalls for web-based applications',\n        'f) Configure secure email encryption and digital signatures',\n        'g) Train users on safe web browsing and email handling practices',\n        'h) Monitor web and email traffic for security threats'\n    ],\n    1\n);\n\n-- Penetration Testing\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'Penetration Testing'),\n    'Penetration Testing and Security Assessment',\n    'Comprehensive security testing including penetration tests and red team exercises',\n    ARRAY[\n        'a) Conduct regular penetration testing by qualified personnel',\n        'b) Perform red team exercises to test detection and response capabilities',\n        'c) Test security controls through authorized simulated attacks',\n        'd) Validate security architecture through adversarial testing',\n        'e) Document and remediate findings from security testing',\n        'f) Coordinate testing activities with operational teams',\n        'g) Use testing results to improve security posture',\n        'h) Maintain testing schedules based on risk assessment'\n    ],\n    1\n);\n\n-- GDPR Unified Compliance\nINSERT INTO unified_requirements (category_id, title, description, sub_requirements, sort_order)\nVALUES (\n    (SELECT id FROM unified_compliance_categories WHERE name = 'GDPR Unified Compliance'),\n    'Comprehensive GDPR Data Protection Implementation',\n    'Complete GDPR compliance framework covering all essential privacy and data protection requirements',\n    ARRAY[\n        'a) Establish lawful basis for all personal data processing activities and maintain processing records',\n        'b) Implement consent mechanisms that are freely given, specific, informed and unambiguous',\n        'c) Design and implement data protection by design and by default in all systems',\n        'd) Establish comprehensive data subject rights fulfillment processes (access, rectification, erasure, portability)',\n        'e) Conduct Data Protection Impact Assessments (DPIA) for high-risk processing activities',\n        'f) Implement personal data breach detection, notification and response procedures',\n        'g) Establish data retention, deletion and cross-border transfer compliance procedures',\n        'h) Designate Data Protection Officer (DPO) where required and define responsibilities',\n        'i) Implement privacy-enhancing technologies and data minimization principles',\n        'j) Establish processor agreements and third-party data protection compliance',\n        'k) Implement age verification and parental consent mechanisms for children',\n        'l) Design transparent privacy notices and cookie consent management',\n        'm) Establish automated decision-making and profiling disclosure procedures',\n        'n) Implement data portability and interoperability mechanisms',\n        'o) Establish supervisory authority liaison and regulatory compliance monitoring'\n    ],\n    1\n);"}	apply_all_22_unified_requirements	payam.razifar@gmail.com	\N
20250701083739	{"-- Add comprehensive ISO 27002:2022 controls (Part 1: A.5 through A.8)\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- A.5 Organizational controls (continuing from existing A.5.1-A.5.5)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.6', 'Contact with special interest groups', 'Appropriate contacts with special interest groups or other specialist security forums and professional associations shall be maintained. Implementation: Identify relevant security forums and professional associations. Establish formal contact procedures with special interest groups. Participate in information sharing initiatives. Maintain current contact information and relationships.', 'Governance & Leadership', 'medium', 50, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.7', 'Threat intelligence', 'Information relating to information security threats shall be collected and analyzed to produce threat intelligence. Implementation: Establish threat intelligence collection processes. Analyze threat information from multiple sources. Integrate threat intelligence into risk management. Share relevant threat information with stakeholders.', 'Risk Management', 'high', 51, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.8', 'Information security in project management', 'Information security shall be integrated into project management. Implementation: Include security requirements in project planning. Conduct security assessments for projects. Assign security responsibilities in project teams. Review security implementation in project deliverables.', 'Governance & Leadership', 'medium', 52, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.9', 'Inventory of information and other associated assets', 'An inventory of information and other associated assets, including owners, shall be drawn up and maintained. Implementation: Identify and inventory all information assets. Assign ownership for each information asset. Maintain asset classification and handling requirements. Regular review and update of information asset inventory.', 'Asset Management', 'high', 53, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.10', 'Acceptable use of information and other associated assets', 'Rules for the acceptable use of information and other associated assets shall be identified, documented and implemented. Implementation: Develop acceptable use policies for information assets. Define prohibited uses and activities. Communicate acceptable use requirements to all users. Monitor compliance with acceptable use policies.', 'Asset Management', 'high', 54, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.11', 'Return of assets', 'Personnel and other parties shall return all organizational assets in their possession upon termination of their employment, contract or agreement. Implementation: Define asset return procedures for termination. Maintain inventory of assets assigned to individuals. Implement asset return verification processes. Ensure secure handling of returned assets.', 'Asset Management', 'high', 55, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.12', 'Classification of information', 'Information shall be classified according to the information security needs of the organization based on confidentiality, integrity, availability and relevant interested party requirements. Implementation: Develop information classification scheme. Assign classification levels based on sensitivity. Define handling requirements for each classification. Train personnel on classification procedures.', 'Data Protection', 'high', 56, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.13', 'Labelling of information', 'An appropriate set of procedures for information labelling shall be developed and implemented in accordance with the information classification scheme adopted by the organization. Implementation: Develop labelling procedures for all information types. Implement consistent labelling across all media. Train users on proper labelling requirements. Monitor compliance with labelling procedures.', 'Data Protection', 'high', 57, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.5.14', 'Information transfer', 'Information transfer rules, procedures or agreements shall be in place for all types of transfer facilities within the organization and between the organization and other parties. Implementation: Establish information transfer policies and procedures. Define secure transfer methods for different information types. Implement transfer agreements with external parties. Monitor and log information transfers.', 'Data Protection', 'high', 58, NOW(), NOW(), true),\n\n-- A.6 People controls (continuing from existing A.6.1-A.6.3)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.6.4', 'Disciplinary process', 'A disciplinary process shall be formalized and communicated to take actions against personnel and other relevant interested parties who have committed an information security breach. Implementation: Establish formal disciplinary procedures for security violations. Define escalation procedures for different violation types. Ensure consistent application of disciplinary measures. Document disciplinary actions and outcomes.', 'Governance & Leadership', 'medium', 59, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.6.5', 'Information security responsibilities after termination or change of employment', 'Information security responsibilities and duties that remain valid after termination or change of employment shall be defined, enforced and communicated to relevant personnel and other interested parties. Implementation: Define post-employment security obligations. Implement confidentiality agreements that survive termination. Monitor compliance with post-employment restrictions. Establish procedures for ongoing security responsibilities.', 'Governance & Leadership', 'high', 60, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.6.6', 'Confidentiality or non-disclosure agreements', 'Confidentiality or non-disclosure agreements reflecting the organization needs for the protection of information shall be identified, regularly reviewed and documented. Implementation: Develop comprehensive confidentiality agreements. Ensure agreements cover all relevant information types. Regular review and update of agreements. Monitor compliance with confidentiality obligations.', 'Governance & Leadership', 'high', 61, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.6.7', 'Remote working', 'Security measures shall be implemented when personnel are working remotely to protect information accessed, processed or stored outside the organization premises. Implementation: Establish remote working security policies. Implement secure remote access technologies. Define acceptable remote working environments. Monitor remote working compliance and security.', 'Physical & Environmental Security Controls', 'high', 62, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.6.8', 'Information security event reporting', 'The organization shall provide a mechanism for personnel to report observed or suspected information security events through appropriate channels in a timely manner. Implementation: Establish incident reporting procedures and channels. Train personnel on incident identification and reporting. Implement anonymous reporting mechanisms. Ensure timely escalation of reported incidents.', 'Incident Response Management', 'high', 63, NOW(), NOW(), true);"}	add_complete_iso27002_controls_part1	payam.razifar@gmail.com	\N
20250701083825	{"-- Add comprehensive ISO 27002:2022 controls (Part 2: A.7 through A.8)\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- A.7 Physical and environmental security (continuing from existing A.7.1-A.7.2)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.3', 'Protection against environmental threats', 'Protection against environmental threats, such as natural disasters, malicious attack or accidents shall be designed and implemented. Implementation: Identify environmental threats to facilities and systems. Implement protection measures against identified threats. Establish environmental monitoring and alerting systems. Develop response procedures for environmental incidents.', 'Physical & Environmental Security Controls', 'high', 70, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.4', 'Working in secure areas', 'Procedures for working in secure areas shall be designed and implemented. Implementation: Define secure area access and working procedures. Implement supervision and monitoring in secure areas. Establish clear desk and screen policies for secure areas. Control removal of items from secure areas.', 'Physical & Environmental Security Controls', 'high', 71, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.5', 'Protection against physical and environmental threats', 'Physical protection against natural disasters, malicious attack, and accidents shall be designed and implemented. Implementation: Assess physical and environmental risks to facilities. Implement appropriate protective measures and controls. Establish monitoring systems for physical security. Develop response procedures for physical security incidents.', 'Physical & Environmental Security Controls', 'high', 72, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.6', 'Working in secure areas', 'Procedures for working in secure areas shall be designed and implemented. Implementation: Define procedures for secure area access and egress. Implement visitor control and escort procedures. Establish material transfer procedures for secure areas. Monitor activities within secure areas.', 'Physical & Environmental Security Controls', 'medium', 73, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.7', 'Clear desk and clear screen', 'Clear desk and clear screen rules shall be defined and appropriately enforced. Implementation: Establish clear desk policies for all work areas. Implement automatic screen locking mechanisms. Define procedures for securing sensitive documents. Train personnel on clear desk and screen requirements.', 'Physical & Environmental Security Controls', 'medium', 74, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.8', 'Equipment siting and protection', 'Equipment shall be sited and protected to reduce the risks from environmental threats and hazards, and opportunities for unauthorized access. Implementation: Position equipment to minimize environmental risks. Implement physical protection for critical equipment. Control access to equipment and cabling. Establish equipment maintenance and monitoring procedures.', 'Physical & Environmental Security Controls', 'high', 75, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.9', 'Security of assets off-premises', 'Off-site assets shall be protected. Implementation: Identify assets located off-premises. Implement security controls for off-site assets. Establish monitoring and management procedures for remote assets. Define incident response for off-site asset security events.', 'Asset Management', 'high', 76, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.10', 'Storage media', 'Storage media shall be managed in accordance with the classification scheme adopted by the organization. Implementation: Implement secure storage media handling procedures. Apply appropriate controls based on information classification. Establish media sanitization and disposal procedures. Control distribution and access to storage media.', 'Asset Management', 'high', 77, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.11', 'Supporting utilities', 'Information processing facilities shall be protected from power failures and other disruptions caused by failures in supporting utilities. Implementation: Implement uninterruptible power supply (UPS) systems. Establish backup power generation capabilities. Monitor utility systems and infrastructure. Develop procedures for utility failure response.', 'Business Continuity & Disaster Recovery Management', 'high', 78, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.12', 'Cabling security', 'Cables carrying or supporting information services shall be protected from interception, interference or damage. Implementation: Protect data and power cabling from unauthorized access. Implement secure cable routing and installation. Monitor cable infrastructure for tampering. Establish procedures for cable maintenance and inspection.', 'Physical & Environmental Security Controls', 'medium', 79, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.13', 'Equipment maintenance', 'Equipment shall be correctly maintained to ensure availability, integrity and confidentiality of information. Implementation: Establish preventive maintenance schedules for critical equipment. Implement secure maintenance procedures. Control access during maintenance activities. Document all maintenance activities and changes.', 'Asset Management', 'medium', 80, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.7.14', 'Secure disposal or reuse of equipment', 'Items of equipment containing storage media shall be verified to ensure that any sensitive data and licensed software has been removed or securely overwritten prior to disposal or reuse. Implementation: Implement secure data sanitization procedures. Verify complete data removal before disposal or reuse. Maintain records of equipment disposal and sanitization. Establish procedures for equipment donation or sale.', 'Asset Management', 'high', 81, NOW(), NOW(), true),\n\n-- A.8 Technology controls (continuing from existing A.8.1-A.8.3)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.8.4', 'Media handling', 'Media shall be handled in accordance with the classification scheme adopted by the organization. Implementation: Implement media handling procedures based on classification. Control media access and distribution. Establish secure media storage and transport procedures. Monitor media usage and disposal.', 'Asset Management', 'high', 82, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.8.5', 'Secure deletion', 'Data stored on storage media shall be securely deleted when no longer required. Implementation: Implement secure deletion procedures for different media types. Use appropriate sanitization methods based on data sensitivity. Verify successful data deletion. Maintain records of data deletion activities.', 'Data Protection', 'high', 83, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.8.6', 'Management of removable media', 'Procedures shall be implemented for the management of removable media in accordance with the classification scheme adopted by the organization. Implementation: Control use of removable media based on classification. Implement technical controls to prevent unauthorized media use. Establish procedures for media registration and tracking. Monitor removable media usage and security.', 'Asset Management', 'high', 84, NOW(), NOW(), true);"}	add_complete_iso27002_controls_part2	payam.razifar@gmail.com	\N
20250701083916	{"-- Add comprehensive ISO 27002:2022 controls (Part 3: A.9 through A.14)\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- A.9 Access control (continuing from existing A.9.1-A.9.2)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.9.3', 'Management of privileged access rights', 'The allocation and use of privileged access rights shall be restricted and controlled. Implementation: Identify and control privileged access requirements. Implement additional authorization for privileged access. Monitor and log privileged access activities. Regularly review privileged access assignments.', 'Identity & Access Management', 'high', 90, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.9.4', 'Management of secret authentication information of users', 'The allocation of secret authentication information shall be controlled through a formal management process. Implementation: Implement secure authentication credential management. Control distribution of authentication credentials. Establish credential change and revocation procedures. Monitor authentication credential usage.', 'Identity & Access Management', 'high', 91, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.9.5', 'Review of user access rights', 'Asset owners shall review users access rights at regular intervals. Implementation: Establish periodic access rights review procedures. Assign responsibility for access rights reviews. Document review findings and corrective actions. Implement automated access review tools where possible.', 'Identity & Access Management', 'high', 92, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.9.6', 'Removal or adjustment of access rights', 'The access rights of all personnel and other users shall be removed upon termination of their employment, contract or agreement, or be adjusted upon change. Implementation: Implement timely access removal procedures for terminations. Adjust access rights based on role changes. Coordinate access changes with HR processes. Monitor compliance with access change procedures.', 'Identity & Access Management', 'high', 93, NOW(), NOW(), true),\n\n-- A.10 Cryptography (continuing from existing A.10.1)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.10.2', 'Key management', 'A policy on the use, protection and lifetime of cryptographic keys shall be developed and implemented through their whole lifecycle. Implementation: Establish comprehensive key management procedures. Implement secure key generation, distribution, and storage. Define key lifecycle management processes. Monitor key usage and implement key rotation.', 'Data Protection', 'high', 94, NOW(), NOW(), true),\n\n-- A.11 Physical and environmental security\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.11.1', 'Physical security perimeters', 'Physical security perimeters shall be defined and used to protect areas that contain either sensitive or critical information and information processing facilities. Implementation: Define and implement physical security perimeters. Control access to secure areas with appropriate mechanisms. Monitor perimeter security continuously. Maintain physical barrier integrity.', 'Physical & Environmental Security Controls', 'high', 95, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.11.2', 'Physical entry controls', 'Secure areas shall be protected by appropriate entry controls to ensure that only authorized personnel are allowed access. Implementation: Implement multi-factor authentication for secure area access. Maintain visitor logs and escort procedures. Monitor all entry and exit activities. Regular review of access authorizations.', 'Physical & Environmental Security Controls', 'high', 96, NOW(), NOW(), true),\n\n-- A.12 Operations security\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.12.1', 'Operational procedures and responsibilities', 'Operating procedures shall be documented and made available to all users who need them. Implementation: Document all operational procedures comprehensively. Assign clear operational responsibilities. Train personnel on operational procedures. Regularly review and update procedures.', 'Governance & Leadership', 'medium', 97, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.12.2', 'Change management', 'Changes to the organization, business processes, information processing facilities and systems that affect information security shall be controlled. Implementation: Establish formal change management procedures. Assess security implications of all changes. Implement change approval and testing processes. Monitor and document all changes.', 'Secure Configuration of Hardware and Software', 'high', 98, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.12.3', 'Capacity management', 'The use of resources shall be monitored, tuned and projections made of future capacity requirements to ensure the required system performance. Implementation: Monitor system capacity and performance continuously. Establish capacity planning procedures. Implement resource optimization strategies. Plan for future capacity requirements.', 'Network Infrastructure Management', 'medium', 99, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.12.4', 'Separation of development, testing and operational environments', 'Development, testing, and operational environments shall be separated to reduce the risks of unauthorized access or changes to the operational environment. Implementation: Implement separate environments for development, testing, and production. Control data flow between environments. Restrict access based on environment and role. Monitor cross-environment activities.', 'Secure Software Development', 'high', 100, NOW(), NOW(), true),\n\n-- A.13 Communications security\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.13.1', 'Network security management', 'Networks shall be managed and controlled to protect information in systems and applications. Implementation: Implement network segmentation and access controls. Monitor network traffic for security threats. Establish network security policies and procedures. Manage network infrastructure securely.', 'Network Infrastructure Management', 'high', 101, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.13.2', 'Information transfer', 'Information involved in information transfer shall remain protected during transmission. Implementation: Implement secure communication protocols. Encrypt sensitive data in transit. Establish secure file transfer procedures. Monitor and log information transfers.', 'Data Protection', 'high', 102, NOW(), NOW(), true),\n\n-- A.14 System acquisition, development and maintenance\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.14.1', 'Security requirements analysis and specification', 'The information security requirements shall be included in the requirements for new information systems or enhancements to existing information systems. Implementation: Include security requirements in all system specifications. Conduct security requirements analysis for new systems. Validate security requirements implementation. Document security requirements and compliance.', 'Secure Software Development', 'high', 103, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.14.2', 'Securing application services on public networks', 'Information involved in application service transactions shall be protected from fraudulent activity, contract dispute and unauthorized disclosure and modification. Implementation: Implement secure authentication for public network applications. Use strong encryption for public network transactions. Implement transaction monitoring and fraud detection. Establish secure application architecture.', 'Network Infrastructure Management', 'high', 104, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.14.3', 'Protecting application services transactions', 'Information involved in application service transactions shall be protected from incomplete transmission, mis-routing, unauthorized message alteration, unauthorized disclosure, unauthorized message duplication or replay. Implementation: Implement transaction integrity controls. Use secure communication protocols for transactions. Implement transaction logging and monitoring. Establish transaction validation procedures.', 'Secure Software Development', 'high', 105, NOW(), NOW(), true);"}	add_complete_iso27002_controls_part3	payam.razifar@gmail.com	\N
20250701084048	{"-- Add additional CIS Controls that don't exist yet\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- CIS Control 2 additional safeguards\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '2.4', 'Utilize Automated Software Inventory Tools', 'Utilize automated software inventory tools throughout the enterprise. Implementation: Deploy enterprise-wide automated software inventory solutions. Configure tools for comprehensive software discovery. Integrate with configuration management systems. Automate software compliance monitoring and reporting.', 'Inventory and Control of Software Assets', 'high', 120, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '2.5', 'Use Technical Controls to Enforce Application Allowlisting', 'Use technical controls, such as application allowlisting, to prevent unauthorized software execution. Implementation: Implement application allowlisting on all enterprise assets. Configure allowlisting to permit only authorized software execution. Establish exception processes for required software. Monitor allowlisting violations.', 'Inventory and Control of Software Assets', 'high', 121, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '2.6', 'Allowlist Authorized Software Libraries', 'Allowlist authorized software libraries, such as specific .dll, .ocx, .so files, which are allowed to be loaded by authorized software. Implementation: Identify and catalog authorized software libraries. Implement library allowlisting controls. Monitor library loading activities. Maintain approved library versions.', 'Inventory and Control of Software Assets', 'high', 122, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '2.7', 'Allowlist Authorized Scripts', 'Allowlist authorized scripts that are allowed to run on enterprise assets. Implementation: Identify and approve all authorized scripts. Implement script execution controls and allowlisting. Monitor script execution activities. Establish secure script development procedures.', 'Inventory and Control of Software Assets', 'high', 123, NOW(), NOW(), true),\n\n-- CIS Control 3 additional safeguards\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.2', 'Establish and Maintain a Data Inventory', 'Establish and maintain a data inventory, based on the enterprise data management process. Implementation: Catalog all data types and repositories. Document data flows and processing activities. Classify data based on sensitivity and business value. Maintain data inventory accuracy through regular updates.', 'Data Protection', 'high', 124, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.3', 'Configure Data Access Control Lists', 'Configure data access control lists based on a users need to know. Implementation: Implement role-based access controls for data. Configure granular permissions based on job functions. Regular review of data access permissions. Monitor data access activities and violations.', 'Data Protection', 'high', 125, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.4', 'Enforce Data Retention', 'Retain data according to the enterprise data management process. Implementation: Establish data retention policies based on legal and business requirements. Implement automated data lifecycle management. Monitor data retention compliance. Secure disposal of data beyond retention period.', 'Data Protection', 'medium', 126, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.5', 'Securely Dispose of Data', 'Securely dispose of data as outlined in the enterprise data management process. Implementation: Implement secure data sanitization procedures. Use appropriate destruction methods for different media types. Verify successful data destruction. Maintain records of data disposal activities.', 'Data Protection', 'high', 127, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.6', 'Encrypt Data on End-User Devices', 'Encrypt data on end-user devices containing sensitive data. Enterprise assets storing or processing sensitive data shall use full disk encryption. Implementation: Deploy full disk encryption on all enterprise endpoints. Implement strong encryption algorithms and key management. Monitor encryption status across all devices. Establish encryption compliance verification.', 'Data Protection', 'high', 128, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.7', 'Establish and Maintain a Data Classification Scheme', 'Establish and maintain an enterprise data classification scheme. Implementation: Develop comprehensive data classification framework. Define classification categories and handling requirements. Train personnel on data classification procedures. Implement automated classification tools where possible.', 'Data Protection', 'high', 129, NOW(), NOW(), true),\n\n-- CIS Control 4 additional safeguards\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '4.3', 'Configure Automatic Session Locking on Enterprise Assets', 'Configure automatic session locking on enterprise assets after a defined period of inactivity. Implementation: Configure automatic screen locking after inactivity period. Set appropriate timeout values based on risk assessment. Require authentication to unlock sessions. Monitor compliance with session locking policies.', 'Secure Configuration of Hardware and Software', 'medium', 130, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '4.4', 'Implement and Manage a Firewall on Servers', 'Implement and manage a firewall on servers, where supported. Implementation: Deploy host-based firewalls on all enterprise servers. Configure firewall rules based on least privilege principle. Monitor firewall logs for security events. Regular review and update of firewall configurations.', 'Network Infrastructure Management', 'high', 131, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '4.5', 'Implement and Manage a Firewall on End-User Devices', 'Implement and manage a firewall on end-user devices, where supported. Implementation: Deploy and configure host-based firewalls on end-user devices. Establish standardized firewall configurations. Monitor firewall status and compliance. Provide user guidance on firewall functionality.', 'Network Infrastructure Management', 'medium', 132, NOW(), NOW(), true);"}	add_additional_cis_controls	payam.razifar@gmail.com	\N
20250701084137	{"-- Add comprehensive GDPR requirements\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- GDPR Article 5 - Principles\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.5.1.a', 'Lawfulness, fairness and transparency', 'Personal data shall be processed lawfully, fairly and in a transparent manner in relation to the data subject. Implementation: Establish lawful basis for all processing activities. Ensure transparency in data collection and processing. Implement fair processing practices. Document lawful basis for each processing purpose.', 'GDPR Unified Compliance', 'high', 200, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.5.1.b', 'Purpose limitation', 'Personal data shall be collected for specified, explicit and legitimate purposes and not further processed in a manner that is incompatible with those purposes. Implementation: Define specific and explicit purposes for data collection. Ensure processing activities align with stated purposes. Conduct compatibility assessments for new processing. Document purpose specifications clearly.', 'GDPR Unified Compliance', 'high', 201, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.5.1.c', 'Data minimisation', 'Personal data shall be adequate, relevant and limited to what is necessary in relation to the purposes for which they are processed. Implementation: Collect only necessary personal data for stated purposes. Regular review of data collection practices. Implement data minimization by design. Remove unnecessary data collection points.', 'Data Protection', 'high', 202, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.5.1.d', 'Accuracy', 'Personal data shall be accurate and, where necessary, kept up to date; every reasonable step must be taken to ensure that personal data that are inaccurate are erased or rectified without delay. Implementation: Implement data accuracy validation procedures. Establish data correction and update processes. Monitor data quality continuously. Provide mechanisms for data subjects to correct inaccuracies.', 'Data Protection', 'high', 203, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.5.1.e', 'Storage limitation', 'Personal data shall be kept in a form which permits identification of data subjects for no longer than is necessary for the purposes for which the personal data are processed. Implementation: Establish data retention policies and schedules. Implement automated data deletion procedures. Monitor data retention compliance. Define criteria for data retention periods.', 'Data Protection', 'high', 204, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.5.1.f', 'Integrity and confidentiality', 'Personal data shall be processed in a manner that ensures appropriate security including protection against unauthorised or unlawful processing and against accidental loss, destruction or damage, using appropriate technical or organisational measures. Implementation: Implement comprehensive data security measures. Use encryption for data protection. Establish access controls and monitoring. Conduct regular security assessments.', 'Data Protection', 'high', 205, NOW(), NOW(), true),\n\n-- GDPR Article 6 - Lawfulness of processing\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.6.1', 'Lawfulness of processing', 'Processing shall be lawful only if and to the extent that at least one of the six lawful bases applies. Implementation: Identify and document lawful basis for each processing activity. Ensure appropriate lawful basis selection. Maintain evidence of lawful basis. Review lawful basis regularly for ongoing processing.', 'GDPR Unified Compliance', 'high', 206, NOW(), NOW(), true),\n\n-- GDPR Article 7 - Conditions for consent\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.7.1', 'Demonstrable consent', 'Where processing is based on consent, the controller shall be able to demonstrate that the data subject has consented to processing of his or her personal data. Implementation: Implement verifiable consent mechanisms. Maintain records of consent. Ensure consent is freely given, specific, informed and unambiguous. Provide clear consent withdrawal options.', 'GDPR Unified Compliance', 'high', 207, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.7.3', 'Withdrawal of consent', 'The data subject shall have the right to withdraw his or her consent at any time. Implementation: Provide easy mechanisms for consent withdrawal. Ensure withdrawal is as easy as giving consent. Stop processing immediately upon consent withdrawal. Inform data subjects of their right to withdraw consent.', 'GDPR Unified Compliance', 'high', 208, NOW(), NOW(), true),\n\n-- GDPR Article 12 - Transparent information\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.12.1', 'Transparent information, communication and modalities', 'The controller shall take appropriate measures to provide any information and any communication relating to processing to the data subject in a concise, transparent, intelligible and easily accessible form. Implementation: Develop clear and understandable privacy notices. Use plain language in all communications. Ensure information is easily accessible. Provide information in appropriate formats.', 'GDPR Unified Compliance', 'high', 209, NOW(), NOW(), true),\n\n-- GDPR Article 13 - Information to be provided\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.13.1', 'Information to be provided where personal data are collected from the data subject', 'Where personal data relating to a data subject are collected from the data subject, the controller shall provide specific information at the time of collection. Implementation: Provide comprehensive information at point of data collection. Include all mandatory information elements. Ensure information is provided at time of collection. Update information when processing changes.', 'GDPR Unified Compliance', 'high', 210, NOW(), NOW(), true),\n\n-- GDPR Article 15 - Right of access\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.15.1', 'Right of access by the data subject', 'The data subject shall have the right to obtain from the controller confirmation as to whether or not personal data concerning him or her are being processed. Implementation: Establish data subject access request procedures. Provide confirmation of processing within one month. Include all required information in responses. Verify identity before providing access.', 'GDPR Unified Compliance', 'high', 211, NOW(), NOW(), true),\n\n-- GDPR Article 16 - Right to rectification\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.16', 'Right to rectification', 'The data subject shall have the right to obtain from the controller without undue delay the rectification of inaccurate personal data concerning him or her. Implementation: Establish procedures for data rectification requests. Respond to rectification requests without undue delay. Verify accuracy of requested changes. Notify recipients of rectifications where applicable.', 'GDPR Unified Compliance', 'high', 212, NOW(), NOW(), true),\n\n-- GDPR Article 17 - Right to erasure\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.17.1', 'Right to erasure (right to be forgotten)', 'The data subject shall have the right to obtain from the controller the erasure of personal data concerning him or her without undue delay where specific grounds apply. Implementation: Establish data erasure procedures and systems. Assess erasure requests against legal grounds. Implement secure data deletion processes. Notify third parties of erasure obligations where applicable.', 'GDPR Unified Compliance', 'high', 213, NOW(), NOW(), true),\n\n-- GDPR Article 18 - Right to restriction\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.18.1', 'Right to restriction of processing', 'The data subject shall have the right to obtain from the controller restriction of processing where specific conditions apply. Implementation: Establish processing restriction procedures. Implement technical means to restrict processing. Mark restricted data appropriately. Notify data subject before lifting restrictions.', 'GDPR Unified Compliance', 'high', 214, NOW(), NOW(), true),\n\n-- GDPR Article 20 - Right to data portability\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.20.1', 'Right to data portability', 'The data subject shall have the right to receive the personal data concerning him or her in a structured, commonly used and machine-readable format. Implementation: Develop data export capabilities in machine-readable formats. Establish data portability procedures. Ensure secure transfer methods. Verify identity before providing portable data.', 'GDPR Unified Compliance', 'high', 215, NOW(), NOW(), true);"}	add_comprehensive_gdpr_requirements	payam.razifar@gmail.com	\N
20250701084228	{"-- Add more comprehensive requirements from multiple frameworks\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- Additional GDPR requirements\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.25.1', 'Data protection by design and by default', 'Taking into account the state of the art, the cost of implementation and the nature, scope, context and purposes of processing, the controller shall implement appropriate technical and organisational measures. Implementation: Integrate data protection into system design from the outset. Implement privacy-enhancing technologies. Ensure default settings provide highest privacy protection. Conduct privacy impact assessments for new systems.', 'Data Protection', 'high', 216, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.32.1', 'Security of processing', 'Taking into account the state of the art, the costs of implementation and the nature, scope, context and purposes of processing as well as the risk of varying likelihood and severity for the rights and freedoms of natural persons, the controller and the processor shall implement appropriate technical and organisational measures. Implementation: Implement state-of-the-art security measures. Conduct risk assessments for processing activities. Use encryption and pseudonymisation where appropriate. Establish incident response procedures.', 'Data Protection', 'high', 217, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.33.1', 'Notification of a personal data breach to the supervisory authority', 'In the case of a personal data breach, the controller shall without undue delay and, where feasible, not later than 72 hours after having become aware of it, notify the personal data breach to the supervisory authority. Implementation: Establish breach detection and notification procedures. Implement 72-hour notification timeline. Maintain breach notification templates and procedures. Train staff on breach identification and escalation.', 'Incident Response Management', 'high', 218, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.34.1', 'Communication of a personal data breach to the data subject', 'When the personal data breach is likely to result in a high risk to the rights and freedoms of natural persons, the controller shall communicate the personal data breach to the data subject without undue delay. Implementation: Assess breach risk to data subjects. Develop data subject notification procedures. Use clear and plain language in breach notifications. Provide recommendations for data subject protection.', 'Incident Response Management', 'high', 219, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.35.1', 'Data protection impact assessment', 'Where a type of processing is likely to result in a high risk to the rights and freedoms of natural persons, the controller shall carry out an assessment of the impact of the envisaged processing operations on the protection of personal data. Implementation: Identify processing requiring DPIA. Conduct systematic risk assessments. Document DPIA findings and mitigation measures. Consult supervisory authority where required.', 'Risk Management', 'high', 220, NOW(), NOW(), true),\n\n-- Additional ISO 27001 Section 8 requirements (Operations)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '8.1', 'Operational planning and control', 'The organization shall plan, implement and control the processes needed to meet information security requirements, and to implement the actions determined to achieve information security objectives. Implementation: Develop operational procedures for all information security processes. Establish operational controls and monitoring. Implement change control for operational procedures. Regular review of operational effectiveness.', 'Governance & Leadership', 'high', 221, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '8.2', 'Information security risk assessment', 'The organization shall perform information security risk assessments at planned intervals or when significant changes are proposed or occur. Implementation: Establish risk assessment procedures and schedules. Conduct risk assessments when changes occur. Document risk assessment results and decisions. Update risk treatment plans based on assessment results.', 'Risk Management', 'high', 222, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '8.3', 'Information security risk treatment', 'The organization shall implement the information security risk treatment plan. Implementation: Execute approved risk treatment plans systematically. Implement selected controls according to specifications. Monitor control implementation progress. Verify effectiveness of implemented controls.', 'Risk Management', 'high', 223, NOW(), NOW(), true),\n\n-- Additional ISO 27001 Section 9 requirements (Performance evaluation)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '9.1', 'Monitoring, measurement, analysis and evaluation', 'The organization shall evaluate the information security performance and the effectiveness of the information security management system. Implementation: Establish monitoring and measurement procedures. Define performance indicators and metrics. Conduct regular analysis of monitoring results. Evaluate ISMS effectiveness systematically.', 'Governance & Leadership', 'medium', 224, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '9.2', 'Internal audit', 'The organization shall conduct internal audits at planned intervals to provide information on whether the information security management system conforms to the organization own requirements and the requirements of this document. Implementation: Establish internal audit program and schedule. Train internal auditors on ISMS requirements. Conduct objective and impartial audits. Report audit findings and recommendations.', 'Governance & Leadership', 'high', 225, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '9.3', 'Management review', 'Top management shall review the organization information security management system at planned intervals to ensure its continuing suitability, adequacy and effectiveness. Implementation: Schedule regular management reviews of ISMS. Prepare comprehensive review inputs and documentation. Make decisions on improvement opportunities. Document management review outcomes and actions.', 'Governance & Leadership', 'high', 226, NOW(), NOW(), true),\n\n-- Additional ISO 27001 Section 10 requirements (Improvement)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '10.1', 'Nonconformity and corrective action', 'When a nonconformity occurs, the organization shall react to the nonconformity and take action to control and correct it. Implementation: Establish nonconformity identification and response procedures. Investigate root causes of nonconformities. Implement corrective actions to prevent recurrence. Monitor effectiveness of corrective actions.', 'Governance & Leadership', 'high', 227, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27001' LIMIT 1), '10.2', 'Continual improvement', 'The organization shall continually improve the suitability, adequacy and effectiveness of the information security management system. Implementation: Establish continual improvement processes and objectives. Identify improvement opportunities systematically. Implement improvement initiatives. Monitor and measure improvement results.', 'Governance & Leadership', 'medium', 228, NOW(), NOW(), true),\n\n-- Additional CIS Controls from Control 9 onwards\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '9.1', 'Ensure Use of Only Fully Supported Browsers and Email Clients', 'Ensure only fully supported browsers and email clients are allowed to execute in the enterprise. Implementation: Maintain inventory of approved browsers and email clients. Remove or block unsupported browser versions. Implement automated updates for supported applications. Monitor browser and email client compliance.', 'Email & Web Browser Protections', 'high', 230, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '9.2', 'Use DNS Filtering Services', 'Use DNS filtering services on all enterprise assets to block access to known malicious domains. Implementation: Deploy DNS filtering solutions enterprise-wide. Configure filtering to block known malicious domains. Maintain updated threat intelligence feeds. Monitor DNS filtering effectiveness and bypass attempts.', 'Email & Web Browser Protections', 'high', 231, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '9.3', 'Maintain and Enforce Network-Based URL Filters', 'Enforce and update network-based URL filters to limit an enterprise asset from connecting to potentially malicious or inappropriate websites. Implementation: Deploy network-based URL filtering solutions. Maintain current malicious and inappropriate website databases. Configure appropriate filtering policies by user group. Monitor and log URL filtering activities.', 'Email & Web Browser Protections', 'high', 232, NOW(), NOW(), true);"}	add_more_comprehensive_requirements	payam.razifar@gmail.com	\N
20250701084321	{"-- Add comprehensive NIS2 Directive requirements\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- NIS2 Article 21 - Cybersecurity risk-management measures\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.21.2.a', 'Cybersecurity risk analysis and information system security policies', 'Essential entities and important entities shall take appropriate and proportionate technical, operational and organisational measures to manage the risks posed to the security of network and information systems. Implementation: Conduct comprehensive cybersecurity risk analysis. Develop information system security policies. Implement risk-based security measures. Regular review and update of risk assessments.', 'Risk Management', 'high', 300, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.21.2.b', 'Incident handling', 'Essential entities and important entities shall have procedures for incident handling. Implementation: Establish comprehensive incident handling procedures. Define incident classification and escalation procedures. Implement incident response team structure. Maintain incident response documentation and reporting.', 'Incident Response Management', 'high', 301, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.21.2.c', 'Business continuity', 'Essential entities and important entities shall ensure business continuity through backup management and disaster recovery procedures, and crisis management. Implementation: Develop comprehensive business continuity plans. Implement backup and disaster recovery procedures. Establish crisis management capabilities. Conduct regular business continuity testing.', 'Business Continuity & Disaster Recovery Management', 'high', 302, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.21.2.d', 'Supply chain security', 'Essential entities and important entities shall address supply chain security, including security-related aspects concerning the relationships between each entity and its direct suppliers or service providers. Implementation: Assess and manage supply chain cybersecurity risks. Implement security requirements for suppliers and service providers. Monitor third-party security compliance. Establish supply chain incident response procedures.', 'Supplier & Third-Party Risk Management', 'high', 303, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.21.2.e', 'Security in network and information systems acquisition, development and maintenance', 'Essential entities and important entities shall implement security measures for network and information systems acquisition, development and maintenance. Implementation: Integrate security into system acquisition processes. Implement secure development lifecycle practices. Establish security requirements for system maintenance. Conduct security assessments for new systems.', 'Secure Software Development', 'high', 304, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.21.2.f', 'Policies and procedures to assess the effectiveness of cybersecurity risk-management measures', 'Essential entities and important entities shall have policies and procedures to assess the effectiveness of cybersecurity risk-management measures. Implementation: Develop effectiveness assessment policies and procedures. Implement regular evaluation of cybersecurity measures. Monitor and measure security control effectiveness. Document assessment results and improvement actions.', 'Governance & Leadership', 'high', 305, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.21.2.g', 'Basic cyber hygiene practices and cybersecurity training', 'Essential entities and important entities shall implement basic cyber hygiene practices and cybersecurity training. Implementation: Establish basic cyber hygiene requirements and practices. Develop comprehensive cybersecurity training programs. Provide regular security awareness training to all personnel. Monitor training effectiveness and compliance.', 'Security Awareness & Skills Training', 'high', 306, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.21.2.h', 'Policies and procedures regarding the use of cryptography and encryption', 'Essential entities and important entities shall have policies and procedures regarding the use of cryptography and encryption. Implementation: Develop comprehensive cryptography and encryption policies. Implement appropriate encryption standards and algorithms. Establish key management procedures. Monitor cryptographic control implementation.', 'Data Protection', 'high', 307, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.21.2.i', 'Human resources security, access control policies and asset management', 'Essential entities and important entities shall implement human resources security, access control policies and asset management. Implementation: Establish human resources security procedures. Implement comprehensive access control policies. Develop asset management procedures and controls. Monitor compliance with security policies.', 'Identity & Access Management', 'high', 308, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.21.2.j', 'Use of multi-factor authentication or continuous authentication solutions', 'Essential entities and important entities shall use multi-factor authentication or continuous authentication solutions, cybersecurity voice and data communication protection. Implementation: Implement multi-factor authentication for all user accounts. Deploy continuous authentication solutions where appropriate. Establish communication protection measures. Monitor authentication system effectiveness.', 'Identity & Access Management', 'high', 309, NOW(), NOW(), true),\n\n-- NIS2 Article 23 - Incident reporting\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.23.1', 'Significant incident reporting', 'Essential entities and important entities shall notify the relevant CSIRT or competent authority of any incident that has a significant impact on the provision of their services. Implementation: Establish incident significance assessment criteria. Implement incident notification procedures. Define reporting timelines and requirements. Maintain incident reporting documentation.', 'Incident Response Management', 'high', 310, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.23.3', 'Early warning notification', 'Essential entities and important entities shall submit an early warning without undue delay after becoming aware of the incident, and in any event within 24 hours. Implementation: Establish 24-hour early warning notification procedures. Implement incident detection and assessment capabilities. Define early warning criteria and thresholds. Maintain emergency contact procedures.', 'Incident Response Management', 'high', 311, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.23.4', 'Incident report', 'Essential entities and important entities shall submit an incident report without undue delay, and in any event within one month of the early warning. Implementation: Develop comprehensive incident reporting procedures. Implement detailed incident investigation capabilities. Establish incident documentation and analysis procedures. Maintain incident reporting templates and standards.', 'Incident Response Management', 'high', 312, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.23.5', 'Final report', 'Essential entities and important entities may be required to submit a final report. Implementation: Establish final incident report procedures. Implement post-incident analysis capabilities. Document lessons learned and improvement actions. Maintain comprehensive incident closure procedures.', 'Incident Response Management', 'medium', 313, NOW(), NOW(), true),\n\n-- NIS2 Article 20 - Management responsibilities\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.20.1', 'Management body approval and oversight', 'Member States shall ensure that the management body of essential entities and important entities approves the cybersecurity risk-management measures and oversees their implementation. Implementation: Ensure management body approval of cybersecurity measures. Establish management oversight of cybersecurity implementation. Document management decisions and oversight activities. Regular management review of cybersecurity posture.', 'Governance & Leadership', 'high', 314, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'NIS2 Directive' LIMIT 1), 'Art.20.2', 'Management body cybersecurity training', 'Member States shall ensure that members of the management body of essential entities and important entities follow cybersecurity training. Implementation: Provide cybersecurity training to management body members. Ensure training covers relevant cybersecurity risks and measures. Maintain training records and evidence. Regular update of management cybersecurity knowledge.', 'Security Awareness & Skills Training', 'high', 315, NOW(), NOW(), true);"}	add_comprehensive_nis2_requirements	payam.razifar@gmail.com	\N
20250701084410	{"-- Add more comprehensive CIS Controls (Controls 10-18)\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- CIS Control 10: Malware Defenses\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '10.1', 'Deploy and Maintain Anti-Malware Software', 'Deploy and maintain anti-malware software on all enterprise assets. Implementation: Install anti-malware software on all enterprise systems. Configure automatic signature updates. Enable real-time scanning capabilities. Centrally manage anti-malware deployment and configuration.', 'Malware Defenses', 'high', 400, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '10.2', 'Configure Automatic Anti-Malware Signature Updates', 'Configure automatic anti-malware signature updates on all enterprise assets. Implementation: Enable automatic signature update mechanisms. Ensure regular signature database updates. Monitor update status across all systems. Implement backup update procedures for isolated systems.', 'Malware Defenses', 'high', 401, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '10.3', 'Disable Autorun and Autoplay for Removable Media', 'Disable autorun and autoplay auto-execute functionality for removable media. Implementation: Disable autorun/autoplay functionality on all enterprise systems. Configure group policy to prevent auto-execution. Monitor compliance with autorun/autoplay restrictions. Train users on safe removable media handling.', 'Malware Defenses', 'high', 402, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '10.4', 'Configure Anti-Malware Scanning of Removable Media', 'Configure anti-malware scanning for removable media. Implementation: Enable automatic scanning of all removable media. Configure appropriate scanning policies and actions. Monitor scanning activities and results. Implement quarantine procedures for infected media.', 'Malware Defenses', 'high', 403, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '10.5', 'Enable Anti-Exploitation Features', 'Enable anti-exploitation features on enterprise assets and software, where possible, such as Microsoft Data Execution Prevention (DEP), Windows Defender Exploit Guard (WDEG), or Apple System Integrity Protection (SIP) and Gatekeeper. Implementation: Enable built-in anti-exploitation features. Configure advanced threat protection mechanisms. Monitor exploitation attempt detection. Maintain current protection feature updates.', 'Malware Defenses', 'high', 404, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '10.6', 'Centrally Manage Anti-Malware Software', 'Centrally manage anti-malware software. Implementation: Deploy centralized anti-malware management console. Configure standardized policies across all systems. Monitor anti-malware status and compliance. Implement centralized reporting and alerting.', 'Malware Defenses', 'high', 405, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '10.7', 'Use Behavior-Based Anti-Malware Software', 'Use behavior-based anti-malware software. Implementation: Deploy behavior-based detection capabilities. Configure heuristic and behavioral analysis. Monitor for suspicious behavioral patterns. Integrate with incident response procedures.', 'Malware Defenses', 'high', 406, NOW(), NOW(), true),\n\n-- CIS Control 11: Data Recovery\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '11.1', 'Establish and Maintain a Data Recovery Process', 'Establish and maintain a data recovery process. In the process, address the scope of data recovery activities, recovery prioritization, and the security of backup data. Implementation: Develop comprehensive data recovery procedures. Define recovery time and point objectives. Establish backup prioritization based on criticality. Implement secure backup storage and access controls.', 'Business Continuity & Disaster Recovery Management', 'high', 407, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '11.2', 'Perform Automated Backups', 'Perform automated backups of in-scope enterprise assets. Implementation: Configure automated backup systems for all critical data. Establish regular backup schedules. Monitor backup completion and success rates. Implement backup verification procedures.', 'Business Continuity & Disaster Recovery Management', 'high', 408, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '11.3', 'Protect Recovery Data', 'Protect recovery data with equivalent controls to the original data. Implementation: Apply same security controls to backup data as production data. Implement encryption for backup data at rest and in transit. Establish access controls for backup systems. Monitor backup data integrity and security.', 'Business Continuity & Disaster Recovery Management', 'high', 409, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '11.4', 'Establish and Maintain an Isolated Instance of Recovery Data', 'Establish and maintain an isolated instance of recovery data. Implementation: Create air-gapped or isolated backup copies. Implement offline backup storage capabilities. Establish procedures for isolated backup access. Regularly test isolated backup recovery capabilities.', 'Business Continuity & Disaster Recovery Management', 'high', 410, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '11.5', 'Test Data Recovery', 'Test data recovery to verify the integrity of the backup as well as the restoration process. Implementation: Conduct regular backup restoration testing. Verify data integrity after recovery. Document test results and lessons learned. Address identified deficiencies in backup procedures.', 'Business Continuity & Disaster Recovery Management', 'high', 411, NOW(), NOW(), true),\n\n-- CIS Control 12: Network Infrastructure Management\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '12.1', 'Ensure Network Infrastructure is Up-to-Date', 'Ensure network infrastructure is kept up-to-date. Implementation: Maintain current firmware and software on all network devices. Implement patch management for network infrastructure. Monitor network device security updates. Establish change control for network infrastructure updates.', 'Network Infrastructure Management', 'high', 412, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '12.2', 'Establish and Maintain a Secure Network Architecture', 'Establish and maintain a secure network architecture. Implementation: Design and implement network segmentation. Establish secure network zones and boundaries. Implement network access controls and filtering. Document network architecture and security controls.', 'Network Infrastructure Management', 'high', 413, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '12.3', 'Securely Manage Network Infrastructure', 'Securely manage network infrastructure. Implementation: Implement secure management protocols for network devices. Use encrypted management channels. Establish privileged access controls for network management. Monitor network management activities.', 'Network Infrastructure Management', 'high', 414, NOW(), NOW(), true),\n\n-- CIS Control 13: Network Monitoring and Defense\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '13.1', 'Centralize Security Event Alerting', 'Centralize security event alerting across enterprise assets for effective monitoring. Implementation: Deploy centralized security information and event management (SIEM) system. Configure event correlation and alerting rules. Establish security event monitoring procedures. Train security personnel on alert triage and response.', 'Network Monitoring & Defense', 'high', 415, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '13.2', 'Deploy a Host-Based Intrusion Detection System', 'Deploy a Host-Based Intrusion Detection System (HIDS) or Host-Based Intrusion Prevention System (HIPS) on enterprise assets. Implementation: Install HIDS/HIPS on critical enterprise systems. Configure detection rules and signatures. Monitor host-based security events. Integrate with centralized monitoring systems.', 'Network Monitoring & Defense', 'high', 416, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '13.3', 'Deploy a Network Intrusion Detection System', 'Deploy a Network Intrusion Detection System (NIDS) or Network Intrusion Prevention System (NIPS). Implementation: Deploy network-based intrusion detection/prevention systems. Configure network monitoring and detection rules. Monitor network traffic for malicious activity. Establish network-based incident response procedures.', 'Network Monitoring & Defense', 'high', 417, NOW(), NOW(), true);"}	add_more_comprehensive_cis_controls	payam.razifar@gmail.com	\N
20250701084503	{"-- Add final batch of CIS Controls (Controls 14-18 complete)\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- CIS Control 14: Security Awareness and Skills Training (continuing from previous)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '14.1', 'Establish and Maintain a Security Awareness Program', 'Establish and maintain a security awareness program. Implementation: Develop comprehensive security awareness program covering all enterprise users. Create role-specific awareness content. Establish regular awareness training schedule. Measure awareness program effectiveness.', 'Security Awareness & Skills Training', 'high', 500, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '14.2', 'Train Workforce Members to Recognize Social Engineering Attacks', 'Train workforce members to recognize social engineering attacks, such as phishing, pretexting, and tailgating. Implementation: Provide social engineering awareness training to all personnel. Conduct simulated phishing and social engineering exercises. Train users to identify and report suspicious communications. Measure training effectiveness through testing.', 'Security Awareness & Skills Training', 'high', 501, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '14.3', 'Train Workforce Members on Authentication Best Practices', 'Train workforce members on authentication best practices. Implementation: Provide training on password security and multi-factor authentication. Educate users on authentication risks and threats. Train on secure authentication practices. Monitor authentication behavior and provide feedback.', 'Security Awareness & Skills Training', 'high', 502, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '14.4', 'Train Workforce on Data Handling Best Practices', 'Train workforce members on data handling best practices. Implementation: Provide data classification and handling training. Educate on data protection requirements and procedures. Train on secure data storage and transmission. Monitor data handling compliance and provide guidance.', 'Security Awareness & Skills Training', 'high', 503, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '14.5', 'Train Workforce Members on Causes of Unintentional Data Exposure', 'Train workforce members on causes of unintentional data exposure. Implementation: Educate users on common causes of data exposure and leakage. Provide training on data loss prevention practices. Train on secure communication and sharing practices. Conduct regular awareness updates on emerging threats.', 'Security Awareness & Skills Training', 'medium', 504, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '14.6', 'Train Workforce Members on Recognizing and Reporting Security Incidents', 'Train workforce members on recognizing and reporting security incidents. Implementation: Provide incident recognition training to all personnel. Educate on incident reporting procedures and channels. Train on incident response roles and responsibilities. Establish incident reporting metrics and feedback.', 'Security Awareness & Skills Training', 'high', 505, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '14.7', 'Train Workforce on How to Identify and Report if Their Enterprise Assets are Missing Security Updates', 'Train workforce on how to identify and report if their enterprise assets are missing security updates. Implementation: Educate users on security update importance and identification. Provide training on update status checking procedures. Train on reporting mechanisms for missing updates. Establish update compliance monitoring and reporting.', 'Security Awareness & Skills Training', 'medium', 506, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '14.8', 'Train Workforce on the Dangers of Connecting to and Transmitting Enterprise Data Over Insecure Networks', 'Train workforce on the dangers of connecting to and transmitting enterprise data over insecure networks. Implementation: Educate users on network security risks and threats. Provide training on secure network connection practices. Train on VPN usage and secure communication protocols. Monitor network usage and provide security guidance.', 'Security Awareness & Skills Training', 'high', 507, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '14.9', 'Conduct Role-Specific Security Awareness and Skills Training', 'Conduct role-specific security awareness and skills training. Implementation: Develop role-specific security training content. Provide specialized training for high-risk roles. Establish competency requirements for security-sensitive positions. Conduct regular skills assessment and training updates.', 'Security Awareness & Skills Training', 'high', 508, NOW(), NOW(), true),\n\n-- CIS Control 15: Service Provider Management\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '15.1', 'Establish and Maintain an Inventory of Service Providers', 'Establish and maintain an inventory of service providers. Implementation: Maintain comprehensive inventory of all service providers and vendors. Document service provider relationships and dependencies. Classify service providers based on risk and criticality. Regular review and validation of service provider inventory.', 'Supplier & Third-Party Risk Management', 'high', 509, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '15.2', 'Establish and Maintain a Service Provider Management Policy', 'Establish and maintain a service provider management policy. Implementation: Develop comprehensive service provider management policies. Define security requirements for service providers. Establish vendor risk assessment procedures. Document service provider management processes and controls.', 'Supplier & Third-Party Risk Management', 'high', 510, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '15.3', 'Classify Service Providers', 'Classify service providers based on the sensitivity of data and the importance of the service they provide. Implementation: Establish service provider classification framework. Assess data sensitivity and service criticality. Apply appropriate security controls based on classification. Regular review of service provider classifications.', 'Supplier & Third-Party Risk Management', 'high', 511, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '15.4', 'Ensure Service Provider Contracts Include Security Requirements', 'Ensure service provider contracts include security requirements. Implementation: Include comprehensive security requirements in all service provider contracts. Define security performance metrics and reporting requirements. Establish incident notification and response obligations. Include audit rights and compliance verification clauses.', 'Supplier & Third-Party Risk Management', 'high', 512, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '15.5', 'Assess Service Providers', 'Assess service providers consistent with the enterprise assessment methodology. Implementation: Conduct comprehensive security assessments of service providers. Use standardized assessment criteria and methodologies. Document assessment findings and risk ratings. Implement remediation plans for identified deficiencies.', 'Supplier & Third-Party Risk Management', 'high', 513, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '15.6', 'Monitor Service Providers', 'Monitor service providers consistent with the enterprise monitoring methodology. Implementation: Establish ongoing monitoring procedures for service providers. Monitor security performance and compliance. Review service provider security reports and metrics. Conduct periodic reassessments of service provider security posture.', 'Supplier & Third-Party Risk Management', 'high', 514, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '15.7', 'Securely Decommission Service Providers', 'Securely decommission service providers. Implementation: Establish secure service provider termination procedures. Ensure complete data return or secure destruction. Remove service provider access and credentials. Conduct post-termination security verification and documentation.', 'Supplier & Third-Party Risk Management', 'medium', 515, NOW(), NOW(), true);"}	add_final_cis_controls_batch	payam.razifar@gmail.com	\N
20250701084555	{"-- Add remaining CIS Controls and additional comprehensive requirements\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- CIS Control 16: Application Software Security\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.1', 'Establish and Maintain a Secure Application Development Process', 'Establish and maintain a secure application development process. Implementation: Integrate security throughout the software development lifecycle (SDLC). Implement secure coding standards and practices. Establish security requirements and design reviews. Train development teams on secure development practices.', 'Secure Software Development', 'high', 600, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.2', 'Establish and Maintain a Process to Accept and Address Software Vulnerabilities', 'Establish and maintain a process to accept and address software vulnerabilities. Implementation: Establish vulnerability disclosure and response procedures. Implement vulnerability assessment and remediation processes. Maintain vulnerability tracking and management systems. Coordinate with security researchers and disclosure communities.', 'Vulnerability Management', 'high', 601, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.3', 'Perform Root Cause Analysis on Security Vulnerabilities', 'Perform root cause analysis on security vulnerabilities. Implementation: Conduct thorough analysis of vulnerability root causes. Identify systematic issues and development process gaps. Implement corrective actions to prevent similar vulnerabilities. Document lessons learned and process improvements.', 'Secure Software Development', 'high', 602, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.4', 'Establish and Manage an Inventory of Third-Party Software Components', 'Establish and manage an inventory of third-party software components. Implementation: Maintain comprehensive inventory of all third-party software components and dependencies. Monitor third-party component vulnerabilities and updates. Implement component license compliance management. Establish third-party component approval and usage policies.', 'Secure Software Development', 'high', 603, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.5', 'Use Up-to-Date and Trusted Third-Party Software Components', 'Use up-to-date and trusted third-party software components. Implementation: Ensure all third-party components are current and supported. Verify component integrity and authenticity. Monitor for component security updates and patches. Implement component vulnerability scanning and assessment.', 'Secure Software Development', 'high', 604, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.6', 'Establish and Maintain a Severity Rating System and Process for Application Vulnerabilities', 'Establish and maintain a severity rating system and process for application vulnerabilities. Implementation: Develop vulnerability severity rating criteria and methodology. Implement consistent vulnerability assessment and rating procedures. Establish remediation timelines based on severity levels. Monitor vulnerability remediation compliance and effectiveness.', 'Vulnerability Management', 'high', 605, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.7', 'Use Standard Hardening Configuration Templates for Application Infrastructure', 'Use standard hardening configuration templates for application infrastructure. Implementation: Develop standardized hardening templates for application infrastructure components. Implement consistent security configurations across application environments. Monitor configuration compliance and drift. Maintain updated hardening templates based on security best practices.', 'Secure Configuration of Hardware and Software', 'high', 606, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.8', 'Separate Production and Non-Production Systems', 'Separate production and non-production systems. Implementation: Implement physical and logical separation between production and non-production environments. Establish different security controls and access procedures for each environment. Control data flow and access between environments. Monitor cross-environment activities and access.', 'Secure Software Development', 'high', 607, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.9', 'Train Developers in Application Security Concepts and Secure Coding', 'Train developers in application security concepts and secure coding. Implementation: Provide comprehensive application security training to all developers. Include secure coding practices and common vulnerability prevention. Conduct regular security training updates and refreshers. Measure training effectiveness through testing and code reviews.', 'Security Awareness & Skills Training', 'high', 608, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.10', 'Apply Static and Dynamic Code Analysis Tools', 'Apply static and dynamic code analysis tools. Implementation: Integrate static application security testing (SAST) tools into development workflows. Implement dynamic application security testing (DAST) for running applications. Configure automated code analysis in CI/CD pipelines. Review and remediate identified security issues.', 'Secure Software Development', 'high', 609, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.11', 'Leverage Vetted Modules or Services for Application Security Components', 'Leverage vetted modules or services for application security components. Implementation: Use established and vetted security libraries and frameworks. Avoid custom implementation of security-critical functions. Implement secure authentication, authorization, and cryptographic modules. Maintain inventory of approved security components.', 'Secure Software Development', 'high', 610, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.12', 'Implement Code-Level Security Checks', 'Implement code-level security checks. Implementation: Implement input validation and output encoding throughout applications. Use parameterized queries to prevent injection attacks. Implement proper error handling and logging. Conduct security-focused code reviews for all applications.', 'Secure Software Development', 'high', 611, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.13', 'Conduct Application Penetration Testing', 'Conduct application penetration testing. Implementation: Perform regular penetration testing of all enterprise applications. Use qualified penetration testing professionals or services. Test both web-based and non-web applications. Document findings and ensure remediation of identified vulnerabilities.', 'Penetration Testing', 'high', 612, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '16.14', 'Conduct Threat Modeling', 'Conduct threat modeling. Implementation: Perform threat modeling for all enterprise applications during design phase. Identify potential threats and attack vectors. Assess risk levels and implement appropriate mitigations. Update threat models when applications or threats change.', 'Secure Software Development', 'high', 613, NOW(), NOW(), true),\n\n-- CIS Control 17: Incident Response Management\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '17.1', 'Designate Personnel to Manage Incident Handling', 'Designate personnel to manage incident handling. Implementation: Assign dedicated personnel to manage security incidents. Establish incident response team structure and responsibilities. Define roles for incident detection, analysis, containment, and recovery. Provide incident response training to designated personnel.', 'Incident Response Management', 'high', 614, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '17.2', 'Establish and Maintain Contact Information for Reporting Security Incidents', 'Establish and maintain contact information for reporting security incidents. Implementation: Maintain current contact information for incident reporting. Establish multiple communication channels for incident notification. Provide clear guidance on when and how to report incidents. Ensure contact information is readily accessible to all personnel.', 'Incident Response Management', 'high', 615, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '17.3', 'Establish and Maintain an Enterprise Process for Reporting Incidents', 'Establish and maintain an enterprise process for reporting incidents. Implementation: Develop comprehensive incident reporting procedures. Define incident classification and severity levels. Establish reporting timelines and escalation procedures. Train all personnel on incident reporting requirements.', 'Incident Response Management', 'high', 616, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '17.4', 'Establish and Maintain an Incident Response Process', 'Establish and maintain an incident response process. Implementation: Develop comprehensive incident response procedures covering detection, analysis, containment, eradication, and recovery. Define incident response team roles and responsibilities. Establish communication procedures during incidents. Document incident response workflows and decision trees.', 'Incident Response Management', 'high', 617, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '17.5', 'Assign Key Roles and Responsibilities', 'Assign key roles and responsibilities. Implementation: Assign specific incident response roles to qualified personnel. Define clear responsibilities for each role during incident response. Establish backup personnel for critical roles. Document role assignments and contact information.', 'Incident Response Management', 'high', 618, NOW(), NOW(), true);"}	add_remaining_cis_controls_and_more	payam.razifar@gmail.com	\N
20250701084659	{"-- Add massive batch of comprehensive requirements (Part 1)\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- Complete CIS Control 17 and 18\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '17.6', 'Define Mechanisms for Communicating During Incident Response', 'Define mechanisms for communicating during incident response. Implementation: Establish multiple communication channels for incident response. Define communication protocols and procedures. Implement secure communication methods for sensitive incident information. Test communication mechanisms regularly.', 'Incident Response Management', 'high', 700, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '17.7', 'Conduct Routine Incident Response Exercises', 'Conduct routine incident response exercises. Implementation: Schedule regular incident response exercises and tabletop simulations. Test incident response procedures and team readiness. Evaluate exercise results and identify improvement opportunities. Update incident response plans based on exercise findings.', 'Incident Response Management', 'high', 701, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '17.8', 'Conduct Post-Incident Reviews', 'Conduct post-incident reviews. Implementation: Perform comprehensive post-incident analysis and review. Document lessons learned and improvement recommendations. Update incident response procedures based on review findings. Share lessons learned across the organization.', 'Incident Response Management', 'high', 702, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '17.9', 'Establish and Maintain Security Incident Thresholds', 'Establish and maintain security incident thresholds. Implementation: Define clear criteria for security incident classification. Establish thresholds for different types of security events. Implement automated alerting based on defined thresholds. Regularly review and update incident thresholds.', 'Incident Response Management', 'medium', 703, NOW(), NOW(), true),\n\n-- CIS Control 18: Penetration Testing\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '18.1', 'Establish and Maintain a Penetration Testing Program', 'Establish and maintain a penetration testing program. Implementation: Develop comprehensive penetration testing program and procedures. Define testing scope, frequency, and methodology. Establish penetration testing team or external provider relationships. Document penetration testing standards and requirements.', 'Penetration Testing', 'high', 704, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '18.2', 'Perform Periodic External Penetration Tests', 'Perform periodic external penetration tests. Implementation: Conduct regular external penetration tests by qualified professionals. Test external-facing systems and applications. Document test findings and remediation requirements. Verify remediation of identified vulnerabilities.', 'Penetration Testing', 'high', 705, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '18.3', 'Remediate Penetration Test Findings', 'Remediate penetration test findings. Implementation: Establish procedures for addressing penetration test findings. Prioritize remediation based on risk and severity. Implement corrective actions and security improvements. Verify successful remediation through re-testing.', 'Penetration Testing', 'high', 706, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '18.4', 'Validate Security Measures', 'Validate security measures. Implementation: Use penetration testing to validate effectiveness of security controls. Test security measures under realistic attack scenarios. Evaluate security architecture and implementation effectiveness. Document validation results and recommendations.', 'Penetration Testing', 'high', 707, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '18.5', 'Perform Periodic Internal Penetration Tests', 'Perform periodic internal penetration tests. Implementation: Conduct regular internal penetration tests to assess internal security posture. Test internal network segmentation and access controls. Evaluate insider threat scenarios and lateral movement capabilities. Document internal penetration test findings and remediation.', 'Penetration Testing', 'high', 708, NOW(), NOW(), true),\n\n-- Additional comprehensive ISO 27002 controls\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.15.1', 'Information security in supplier relationships', 'Information security requirements for mitigating the risks associated with supplier access to the organization assets shall be agreed with the supplier and documented. Implementation: Establish information security requirements for all suppliers. Include security obligations in supplier contracts. Assess supplier security capabilities before engagement. Monitor supplier security compliance regularly.', 'Supplier & Third-Party Risk Management', 'high', 800, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.15.2', 'Addressing security within supplier agreements', 'All relevant information security requirements shall be established and agreed with each supplier that may access, process, store, communicate, or provide IT infrastructure components for, the organization information. Implementation: Define comprehensive security requirements for supplier agreements. Address data protection and confidentiality requirements. Include incident notification and response obligations. Establish audit rights and compliance verification procedures.', 'Supplier & Third-Party Risk Management', 'high', 801, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.16.1', 'Management of information security incidents and improvements', 'Management responsibilities and procedures shall be established to ensure a quick, effective and orderly response to information security incidents. Implementation: Establish incident management procedures and responsibilities. Define incident classification and escalation procedures. Implement incident response team structure. Monitor incident response effectiveness and implement improvements.', 'Incident Response Management', 'high', 802, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.16.2', 'Reporting information security events', 'Information security events shall be reported through appropriate management channels as quickly as possible. Implementation: Establish event reporting procedures and channels. Train personnel on event identification and reporting. Implement timely escalation of security events. Monitor event reporting compliance and effectiveness.', 'Incident Response Management', 'high', 803, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.16.3', 'Reporting information security weaknesses', 'All personnel and contractors using the organization information systems and services shall be required to note and report any observed or suspected information security weaknesses in systems or services. Implementation: Establish weakness reporting procedures. Train personnel on weakness identification. Implement reporting channels for security weaknesses. Address reported weaknesses promptly and systematically.', 'Incident Response Management', 'medium', 804, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.16.4', 'Assessment of and decision on information security events', 'Information security events shall be assessed and it shall be decided if they are to be classified as information security incidents. Implementation: Establish event assessment procedures and criteria. Define incident classification methodology. Implement decision-making processes for incident classification. Document assessment decisions and rationale.', 'Incident Response Management', 'high', 805, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.16.5', 'Response to information security incidents', 'Information security incidents shall be responded to in accordance with documented procedures. Implementation: Develop comprehensive incident response procedures. Implement incident containment and eradication procedures. Establish incident recovery and restoration processes. Document incident response activities and outcomes.', 'Incident Response Management', 'high', 806, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.16.6', 'Learning from information security incidents', 'Knowledge gained from analyzing and resolving information security incidents shall be used to reduce the likelihood or impact of future incidents. Implementation: Conduct post-incident analysis and review. Identify lessons learned and improvement opportunities. Update security controls and procedures based on incident analysis. Share lessons learned across the organization.', 'Incident Response Management', 'medium', 807, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.16.7', 'Collection of evidence', 'The organization shall define and apply procedures for the identification, collection, acquisition and preservation of information, which can serve as evidence. Implementation: Establish evidence collection and preservation procedures. Train personnel on forensic evidence handling. Implement chain of custody procedures. Coordinate with law enforcement when required.', 'Incident Response Management', 'high', 808, NOW(), NOW(), true),\n\n-- Additional comprehensive ISO 27002 A.17 Business continuity\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.17.1', 'Planning information security continuity', 'The organization shall plan and prepare for managing information security during an adverse situation by having a business continuity management system for information security. Implementation: Develop information security continuity planning. Integrate security into business continuity management. Establish security requirements for continuity operations. Define security roles during business continuity events.', 'Business Continuity & Disaster Recovery Management', 'high', 809, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.17.2', 'Implementing information security continuity', 'The organization shall establish, document, implement and maintain processes, procedures and controls to ensure the required level of continuity for information security during an adverse situation. Implementation: Implement information security continuity procedures. Establish backup systems and alternative processing capabilities. Test continuity procedures and controls regularly. Maintain continuity documentation and updates.', 'Business Continuity & Disaster Recovery Management', 'high', 810, NOW(), NOW(), true);"}	add_massive_requirements_batch_1	payam.razifar@gmail.com	\N
20250701084802	{"-- Add massive batch of comprehensive requirements (Part 2)\nINSERT INTO requirements_library (id, standard_id, control_id, title, description, category, priority, order_index, created_at, updated_at, is_active) VALUES\n\n-- Additional comprehensive ISO 27002 A.18 Compliance\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.18.1', 'Compliance with legal and contractual requirements', 'All relevant legislative, statutory, regulatory, contractual requirements and the organization approach to meet these requirements shall be explicitly identified, documented and kept up to date for each information system and the organization. Implementation: Identify all applicable legal and regulatory requirements. Maintain current inventory of compliance obligations. Implement compliance monitoring and reporting procedures. Regular review of compliance requirements and obligations.', 'Governance & Leadership', 'high', 900, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'ISO/IEC 27002' LIMIT 1), 'A.18.2', 'Information security reviews', 'The organization approach to managing information security and its implementation shall be reviewed independently at planned intervals or when significant changes occur. Implementation: Establish independent review procedures for information security. Conduct regular security assessments and audits. Implement management review of security effectiveness. Document review findings and improvement actions.', 'Governance & Leadership', 'high', 901, NOW(), NOW(), true),\n\n-- Additional comprehensive GDPR requirements\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.28.1', 'Processor obligations', 'Where processing is to be carried out on behalf of a controller, the controller shall use only processors providing sufficient guarantees to implement appropriate technical and organisational measures. Implementation: Select processors that provide adequate security guarantees. Implement written contracts with all data processors. Define processor obligations and responsibilities. Monitor processor compliance with data protection requirements.', 'Supplier & Third-Party Risk Management', 'high', 902, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.30.1', 'Records of processing activities', 'Each controller shall maintain a record of processing activities under its responsibility. Implementation: Maintain comprehensive records of all processing activities. Document purposes, categories of data subjects and data. Record retention periods and security measures. Regularly review and update processing records.', 'GDPR Unified Compliance', 'high', 903, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.37.1', 'Designation of the data protection officer', 'The controller and the processor shall designate a data protection officer where processing is carried out by a public authority or where core activities require regular monitoring. Implementation: Assess requirement for Data Protection Officer designation. Designate qualified DPO where required. Ensure DPO independence and adequate resources. Involve DPO in data protection matters.', 'GDPR Unified Compliance', 'high', 904, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'GDPR' LIMIT 1), 'Art.44', 'General principle for transfers', 'Any transfer of personal data to a third country or an international organisation shall take place only if the conditions laid down in this Chapter are complied with by the controller and processor. Implementation: Assess legal basis for international data transfers. Implement appropriate transfer mechanisms and safeguards. Monitor compliance with transfer restrictions. Document transfer decisions and safeguards.', 'Data Protection', 'high', 905, NOW(), NOW(), true),\n\n-- Additional comprehensive CIS Controls from earlier control families (more granular)\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.8', 'Document Data Flows', 'Document data flows. Implementation: Map and document all enterprise data flows. Identify data sources, processing locations, and destinations. Document data transformation and storage points. Maintain current data flow documentation.', 'Data Protection', 'medium', 906, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.9', 'Encrypt Data on Removable Media', 'Encrypt data on removable media. Implementation: Require encryption for all data stored on removable media. Implement strong encryption algorithms and key management. Monitor removable media usage and encryption compliance. Provide guidance on secure removable media handling.', 'Data Protection', 'high', 907, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.10', 'Encrypt Sensitive Data in Transit', 'Encrypt sensitive data in transit. Implementation: Implement encryption for all sensitive data transmissions. Use strong encryption protocols and algorithms. Monitor data transmission security compliance. Establish secure communication standards and procedures.', 'Data Protection', 'high', 908, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.11', 'Encrypt Sensitive Data at Rest', 'Encrypt sensitive data at rest. Implementation: Implement encryption for all sensitive data storage. Use appropriate encryption algorithms and key management. Monitor data-at-rest encryption compliance. Establish secure data storage standards.', 'Data Protection', 'high', 909, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.12', 'Segment Data Processing and Storage Based on Classification', 'Segment data processing and storage based on classification. Implementation: Implement network and system segmentation based on data classification. Separate processing environments for different data sensitivity levels. Control access between different classification segments. Monitor cross-segment data access and transfers.', 'Data Protection', 'high', 910, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.13', 'Deploy a Data Loss Prevention Solution', 'Deploy a Data Loss Prevention (DLP) solution. Implementation: Implement comprehensive DLP solution across enterprise assets. Configure DLP policies based on data classification. Monitor data exfiltration attempts and policy violations. Establish DLP incident response procedures.', 'Data Protection', 'high', 911, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '3.14', 'Log Sensitive Data Access', 'Log sensitive data access. Implementation: Implement comprehensive logging for all sensitive data access. Monitor data access patterns and anomalies. Maintain audit trails for data access activities. Establish data access monitoring and alerting procedures.', 'Data Protection', 'medium', 912, NOW(), NOW(), true),\n\n-- Additional CIS Control 5 and 6 safeguards\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '5.3', 'Disable Dormant Accounts', 'Delete or disable any dormant accounts after a period of 45 days of inactivity, where supported. Implementation: Identify dormant and inactive accounts systematically. Implement automated account disabling for dormant accounts. Establish procedures for account reactivation when needed. Monitor account usage and activity regularly.', 'Identity & Access Management', 'high', 913, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '5.4', 'Restrict Administrator Privileges to Dedicated Administrator Accounts', 'Restrict administrator privileges to dedicated administrator accounts on enterprise assets. Implementation: Create separate administrator accounts for privileged activities. Prohibit use of administrator accounts for routine activities. Implement just-in-time administrative access where possible. Monitor administrative account usage and activities.', 'Identity & Access Management', 'high', 914, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '5.5', 'Establish and Maintain an Inventory of Service Accounts', 'Establish and maintain an inventory of service accounts. Implementation: Identify and catalog all service accounts across enterprise systems. Document service account purposes and owners. Implement service account lifecycle management. Monitor service account usage and activities.', 'Identity & Access Management', 'medium', 915, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '5.6', 'Centralize Account Management', 'Centralize account management through a directory service or identity provider, where supported. Implementation: Implement centralized identity and access management system. Integrate all enterprise systems with central directory. Establish single sign-on capabilities where appropriate. Monitor centralized authentication activities.', 'Identity & Access Management', 'high', 916, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '6.3', 'Require MFA for Externally-Exposed Applications', 'Require multi-factor authentication (MFA) for externally-exposed applications. Implementation: Implement MFA for all internet-facing applications and services. Use strong authentication factors (something you know, have, are). Monitor MFA effectiveness and bypass attempts. Establish MFA policies and user guidance.', 'Identity & Access Management', 'high', 917, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '6.4', 'Require MFA for Remote Network Access', 'Require multi-factor authentication (MFA) for remote network access. Implementation: Implement MFA for all VPN and remote access connections. Use certificate-based or token-based authentication where possible. Monitor remote access activities and anomalies. Establish remote access security policies.', 'Identity & Access Management', 'high', 918, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '6.5', 'Require MFA for Administrative Access', 'Require multi-factor authentication (MFA) for administrative access. Implementation: Implement MFA for all administrative and privileged accounts. Use strong authentication methods for high-privilege access. Monitor administrative access activities. Establish privileged access management procedures.', 'Identity & Access Management', 'high', 919, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '6.6', 'Establish and Maintain an Access Provisioning Process', 'Establish and maintain an access provisioning process. Implementation: Define formal access provisioning procedures and workflows. Implement role-based access provisioning. Establish approval processes for access requests. Monitor access provisioning compliance and effectiveness.', 'Identity & Access Management', 'high', 920, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '6.7', 'Establish and Maintain a Process for Revoking Access', 'Establish and maintain a process for revoking access. Implementation: Define formal access revocation procedures for terminations and role changes. Implement timely access removal processes. Coordinate with HR for termination notifications. Monitor access revocation compliance and completeness.', 'Identity & Access Management', 'high', 921, NOW(), NOW(), true),\n\n(uuid_generate_v4(), (SELECT id FROM standards_library WHERE name = 'CIS Controls Implementation Group 3 (IG3)' LIMIT 1), '6.8', 'Define and Maintain Role-Based Access Control', 'Define and maintain role-based access control, through determining and documenting the access rights necessary for each role within the enterprise to successfully carry out its assigned duties. Implementation: Define comprehensive role-based access control framework. Document access rights for each organizational role. Implement least privilege access principles. Regular review and validation of role definitions and assignments.', 'Identity & Access Management', 'high', 922, NOW(), NOW(), true);"}	add_massive_requirements_batch_2	payam.razifar@gmail.com	\N
