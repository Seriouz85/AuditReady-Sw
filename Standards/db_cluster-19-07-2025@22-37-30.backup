--
-- Name: transfer_assessment_notes_on_completion(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.transfer_assessment_notes_on_completion() RETURNS trigger
    LANGUAGE plpgsql
    SET search_path TO 'public'
    AS $$
DECLARE
  req_record RECORD;
  note_entry JSONB;
BEGIN
  -- Only trigger when assessment status changes to 'completed'
  IF NEW.status = 'completed' AND OLD.status != 'completed' THEN
    
    -- Loop through all assessment requirements that have assessment notes
    FOR req_record IN 
      SELECT ar.requirement_id, ar.assessment_notes, ar.notes
      FROM assessment_requirements ar
      WHERE ar.assessment_id = NEW.id 
      AND JSONB_ARRAY_LENGTH(COALESCE(ar.assessment_notes, '[]'::jsonb)) > 0
    LOOP
      -- Create a consolidated note entry with timestamp
      note_entry := jsonb_build_object(
        'assessment_id', NEW.id,
        'assessment_name', NEW.name,
        'completion_date', NEW.end_date,
        'notes', req_record.assessment_notes,
        'transferred_at', NOW()
      );
      
      -- Update the requirement's global notes in requirements_library
      UPDATE requirements_library 
      SET 
        notes = COALESCE(notes, '') || E'\n\n' || 
               'Assessment Completion Notes (' || TO_CHAR(NOW(), 'YYYY-MM-DD HH24:MI') || '):\n' ||
               'From Assessment: ' || NEW.name || E'\n' ||
               COALESCE(req_record.notes, 'No specific notes provided'),
        updated_at = NOW()
      WHERE id = req_record.requirement_id;
      
    END LOOP;
    
    RAISE NOTICE 'Assessment notes transferred to requirements on completion of assessment: %', NEW.name;
  END IF;
  
  RETURN NEW;
END;
$$;


